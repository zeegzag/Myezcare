<div id="PatientMedicaidList" class="row">
    <div class="col-md-12">
        <div class="portlet box">
            <div class="portlet-title">
                <div class="caption header-font-size-14 uppercase">
                    Patient Medicaid
                    <div class="pull-right padding-left-25" data-ng-show="PatientMedicaidListAjaxStart">
                        <img src="~/Assets/images/lazyload.gif" />
                    </div>
                </div>
                <div class="tools">
                    <a href="javascript:;" class="collapse"></a>
                    <a href="javascript:;" class="fullscreen"></a>
                </div>
            </div>

            <div class="portlet-body table_font-size-12">

                <div class="row">
                    <div class="col-sm-12 form-group  padding-right-15" data-ng-if="PatientMedicaidList.length > 0">
                        <div class="pull-right padding-right-5">
                            <dir-pagination-controls boundary-links="true" on-page-change="PatientMedicaidListPager.pageChanged(newPageNumber)" pagination-id="PatientMedicaidListPagerID"></dir-pagination-controls>
                        </div>
                    </div>
                </div>
