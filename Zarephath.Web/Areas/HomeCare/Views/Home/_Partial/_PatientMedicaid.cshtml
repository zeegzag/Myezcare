                <div class="row">
                    <div class="col-sm-12">
                        <div class="portlet-body" style="display: block;">
                            <div class="table-responsive">
                                <form id="frmReferralinterMessageList">
                                    <table class="table table-bordered table-striped table-condensed flip-content">
                                        <thead>
                                            <tr>
                                                <th>
                                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="PatientMedicaidListPager.sortColumn('Patient')">@Resource.Patient</a>
                                                    <span class="sortorder" data-ng-show="PatientMedicaidListPager.sortIndex === 'Patient'" data-ng-class="{reverse:PatientMedicaidListPager.reverse}"></span>
                                                </th>
                                                <th>
                                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="PatientMedicaidListPager.sortColumn('Dose')">Dose</a>
                                                    <span class="sortorder" data-ng-show="PatientMedicaidListPager.sortIndex === 'Dose'" data-ng-class="{reverse:PatientMedicaidListPager.reverse}"></span>
                                                </th>
                                                <th>
                                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="PatientMedicaidListPager.sortColumn('Unit')">@Resource.Unit</a>
                                                    <span class="sortorder" data-ng-show="PatientMedicaidListPager.sortIndex === 'Unit'" data-ng-class="{reverse:PatientMedicaidListPager.reverse}"></span>
                                                </th>
                                                <th>
                                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="PatientMedicaidListPager.sortColumn('Frequency')">@Resource.Frequency</a>
                                                    <span class="sortorder" data-ng-show="PatientMedicaidListPager.sortIndex === 'Frequency'" data-ng-class="{reverse:PatientMedicaidListPager.reverse}"></span>
                                                </th>
                                                <th>
                                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="PatientMedicaidListPager.sortColumn('Route')">Route</a>
                                                    <span class="sortorder" data-ng-show="PatientMedicaidListPager.sortIndex === 'Route'" data-ng-class="{reverse:PatientMedicaidListPager.reverse}"></span>
                                                </th>
                                                <th>
                                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="PatientMedicaidListPager.sortColumn('CreatedDate')">@Resource.CreatedDate</a>
                                                    <span class="sortorder" data-ng-show="PatientMedicaidListPager.sortIndex === 'CreatedDate'" data-ng-class="{reverse:PatientMedicaidListPager.reverse}"></span>
                                                </th>

                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr dir-paginate="item in PatientMedicaidList | itemsPerPage: PatientMedicaidListPager.pageSize" total-items="PatientMedicaidListPager.totalRecords" current-page="PatientMedicaidListPager.currentPage" pagination-id="PatientMedicaidListPagerID">
                                                <td>
                                                    <a href="@Constants.HC_AddReferral{{item.EncryptedReferralID}}" target="_blank">{{item.Patient}}</a>
                                                </td>
                                                <td>
                                                    <span data-ng-bind="item.Dose"></span>
                                                </td>
                                                <td>
                                                    <span data-ng-bind="item.Unit"></span>
                                                </td>
                                                <td>
                                                    <span data-ng-bind="item.Frequency"></span>
                                                </td>
                                                <td>
                                                    <span data-ng-bind="item.Route"></span>
                                                </td>
                                                <td>
                                                    <span data-ng-bind="item.CreatedDate"></span>
                                                </td>


                                            </tr>
                                        </tbody>
                                    </table>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-sm-12" data-ng-if="PatientMedicaidList.length > 0">
                        <div class="col-sm-6 pull-left no-left-padding"
                             data-ng-hide="PatientMedicaidListPager.currentPage==1 && (PatientMedicaidListPager.currentPageSize < PatientMedicaidListPager.pageSize)">
                            @Resource.Showing <span data-ng-bind="(PatientMedicaidListPager.pageSize * PatientMedicaidListPager.currentPage) -PatientMedicaidListPager.pageSize  + 1"></span>
                            &nbsp;@Resource.To <span data-ng-bind="(PatientMedicaidListPager.pageSize == PatientMedicaidListPager.currentPageSize) ? (PatientMedicaidListPager.pageSize * PatientMedicaidListPager.currentPage) :(PatientMedicaidListPager.pageSize * PatientMedicaidListPager.currentPage) - PatientMedicaidListPager.pageSize  + PatientMedicaidListPager.currentPageSize"></span>&nbsp;@Resource.Of &nbsp;<span data-ng-bind="PatientMedicaidListPager.totalRecords"></span>&nbsp;@Resource.Records
                        </div>
                        <div class="pull-right">
                            <dir-pagination-controls boundary-links="true" on-page-change="PatientMedicaidListPager.pageChanged(newPageNumber)" pagination-id="PatientMedicaidListPagerID"></dir-pagination-controls>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-sm-12">
                        <div data-ng-if="PatientMedicaidList.length == 0">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="alert alert-danger">
                                        @Html.Raw(Resource.NoRecordsFound)
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <br class="clearfix" />
            </div>
        </div>
    </div>

</div>
