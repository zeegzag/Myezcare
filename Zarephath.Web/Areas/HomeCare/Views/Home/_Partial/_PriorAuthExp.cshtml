<div id="PriorAuthExpiringList" class="row">
    <div class="col-md-12">
        <div class="portlet box">
            <div class="portlet-title">
                <div class="caption header-font-size-14 uppercase">
                    Prior Auth Expiring
                    <div class="pull-right padding-left-25" data-ng-show="PriorAuthExpiringListAjaxStart">
                        <img src="~/Assets/images/lazyload.gif" />
                    </div>
                </div>
                <div class="tools">
                    <a href="javascript:;" class="collapse"></a>
                    <a href="javascript:;" class="fullscreen"></a>
                </div>
            </div>

            <div class="portlet-body table_font-size-12">
                <div class="row">
                    <div class="col-sm-12 form-group  padding-right-15" data-ng-if="PriorAuthExpiringList.length > 0">
                        <div class="pull-right padding-right-5">
                            <dir-pagination-controls boundary-links="true" on-page-change="PriorAuthExpiringListPager.pageChanged(newPageNumber)" pagination-id="PriorAuthExpiringListPagerID"></dir-pagination-controls>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-sm-12">
                        <div class="portlet-body" style="display: block;">
                            <div class="table-responsive">
                                <form id="frmReferralinterMessageList">
                                    <table class="table table-bordered table-striped table-condensed flip-content">
                                        <thead>
                                            <tr>
                                                <th>
                                                    @Resource.Patient
                                                </th>
                                                <th>
                                                    @Resource.Payor
                                                </th>
                                                <th>
                                                    Expiring Date
                                                </th>
                                                <th>
                                                    Total Visits
                                                </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr dir-paginate="item in PriorAuthExpiringList | itemsPerPage: PriorAuthExpiringListPager.pageSize" total-items="PriorAuthExpiringListPager.totalRecords" current-page="PriorAuthExpiringListPager.currentPage" pagination-id="PriorAuthExpiringListPagerID">
                                                <td>
                                                    <span data-ng-bind="item.Patient"></span>
                                                </td>
                                                <td>
                                                    <span data-ng-bind="item.PayorName"></span>
                                                </td>
                                                <td>
                                                    <span data-ng-bind="item.ExpireDate | orgdate"></span>
                                                </td>
                                                <td>
                                                    <span data-ng-bind="item.TotalVisits"></span>
                                                </td>

                                            </tr>
                                        </tbody>
                                    </table>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-sm-12" data-ng-if="PriorAuthExpiringList.length > 0">
                        <div class="col-sm-6 pull-left no-left-padding"
                             data-ng-hide="PriorAuthExpiringListPager.currentPage==1 && (PriorAuthExpiringListPager.currentPageSize < PriorAuthExpiringListPager.pageSize)">
                            @Resource.Showing <span data-ng-bind="(PriorAuthExpiringListPager.pageSize * PriorAuthExpiringListPager.currentPage) -PriorAuthExpiringListPager.pageSize  + 1"></span>
                            &nbsp;@Resource.To <span data-ng-bind="(PriorAuthExpiringListPager.pageSize == PriorAuthExpiringListPager.currentPageSize) ? (PriorAuthExpiringListPager.pageSize * PriorAuthExpiringListPager.currentPage) :(PriorAuthExpiringListPager.pageSize * PriorAuthExpiringListPager.currentPage) - PriorAuthExpiringListPager.pageSize  + PriorAuthExpiringListPager.currentPageSize"></span>&nbsp;@Resource.Of &nbsp;<span data-ng-bind="PriorAuthExpiringListPager.totalRecords"></span>&nbsp;@Resource.Records
                        </div>
                        <div class="pull-right">
                            <dir-pagination-controls boundary-links="true" on-page-change="PriorAuthExpiringListPager.pageChanged(newPageNumber)" pagination-id="PriorAuthExpiringListPagerID"></dir-pagination-controls>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-sm-12">
                        <div data-ng-if="PriorAuthExpiringList.length == 0">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="alert alert-danger">
                                        @Html.Raw(Resource.NoRecordsFound)
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <br class="clearfix" />
            </div>
        </div>
    </div>

</div>
