@model DmasModel
@Html.Hidden("hdnDmas97AbModel", Common.SerializeObject(Model))
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12">
        <div id="tabs">
            <div class="tabbable tabbable-custom tabs-left">
                <div class="col-lg-12 col-md-12 col-sm-12 employeeTabsecond">
                    <ul class="nav nav-tabs tabs-left">

                        @if (Common.HasPermission(Constants.HC_Permission_PatientIntake_DMAS97_View) || Common.HasPermission(Constants.HC_Permission_PatientIntake_DMAS97_Add))
                        {
                            <li class="active">
                                <a id="dmas-97" href="#tab_DMAS97" data-toggle="tab">DMAS 97</a>
                            </li>
                        }
                        @if (Common.HasPermission(Constants.HC_Permission_PatientIntake_DMAS99_View) ||Common.HasPermission(Constants.HC_Permission_PatientIntake_DMAS99_Add))
                        {
                            <li class="">
                                <a id="dmas-99" href="#tab_DMAS99" data-toggle="tab">DMAS 99</a>
                            </li>
                        }
                        @if (Common.HasPermission(Constants.HC_Permission_PatientIntake_CMS485_View) || Common.HasPermission(Constants.HC_Permission_PatientIntake_CMS485_Add))
                        {
                            <li class="">
                                <a id="cms-485" href="#tab_CMS485" data-toggle="tab">CMS 485</a>
                            </li>
                        }
                        </ul>
                </div>


                <div class="tab-content col-lg-12 col-md-12 col-sm-12">

                    <div class="tab-pane active" id="tab_DMAS97" style="min-height: 350px;">
                        @Html.Partial("Partial/ReferralForms/_tabDmas97Ab", Model)
                    </div>

                    @{
                        GetDmas99Model model = new GetDmas99Model();
                        <div class="tab-pane" id="tab_DMAS99" style="min-height: 350px;">
                            @Html.Partial("Partial/ReferralForms/_tabDmas99", model)
                        </div>
                    }

                    @{ 
                        GetCms485Model cmsmodel = new GetCms485Model();
                    <div class="tab-pane" id="tab_CMS485" style="min-height: 350px;">
                        @Html.Partial("Partial/ReferralForms/_tabCms485", cmsmodel)
                    </div>
                    }

                </div>

            </div>
        </div>
    </div>
</div>





