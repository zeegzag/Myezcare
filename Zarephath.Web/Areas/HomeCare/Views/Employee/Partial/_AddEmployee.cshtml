                                                                        new
                                                                        {
                                                                            data_ng_model = "EmployeeModel.Employee.FacilityID",
                                                                            data_ng_init = "EmployeeModel.Employee.FacilityID=EmployeeModel.Employee.FacilityID==''?null:EmployeeModel.Employee.FacilityID.toString()",
                                                                            @class = "form-control input-sm"
                                                                        })*@
                                                                    <select class="width100 form-control  input-sm requiredStrip" multiple title="Select Facility"
                                                                            data-val="true" selectpicker="EmployeeModel.Employee.FacilityID" name="EmployeeModel.Employee.FacilityID">
                                                                        <option data-ng-repeat="option in FacilityList" value="{{option.FacilityID}}">{{option.FacilityName}}</option>
                                                                    </select>
                                                                    @Html.ValidationMessageFor(q => q.Employee.Designation)
                                                                </div>
                                                            </div>
                                                        </div>
