@{
    /**/

    bool isEditMode = Model != null && Model.Employee.EmployeeID > 0;
    bool isOrgHasAggregator = Common.IsOrgHasAggregator();
}
@model HC_AddEmployeeModel
<style>
    .bootstrap-select.width100:not([class*="col-"]) {
        width: 100% !important;
    }
</style>
<div id="tab_AddEmployee" class="tab-pane fade in active">
    <form class="form-horizontal" id="frmAddEmployee">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div id="tabs" class="addEmployeeTabs">
                    <div class="tabbable tabbable-custom tabs-left">
                        <div class="col-lg-12 col-md-12 col-sm-12 no-padding employeeTabsecond">
                            <ul class="nav nav-tabs tabs-left">
                                <li id="s1" class="active">
                                    <a id="addEmployee_Information" href="#tab_EmployeeInformation" data-toggle="tab">
                                        @Resource.EmployeeInformation
                                    </a>
                                </li>
                                <li id="s2" class="tab-pane" style="margin-right: 10px;">
                                    <a id="addEmployee_EmployeeDetail" href="#tab_EmployeeDetail" data-toggle="tab">
                                        Employee Details
                                    </a>
                                </li>
                                @if (Common.HasPermission(Constants.HC_Permission_Employee_AddUpdate))
                                {
                                    <li id="s3" class="tab-pane" style="margin-right: 10px;">
                                        <a id="addEmployee_ContactDetail" href="#tab_ContactDetail" data-toggle="tab">
                                            @Resource.EmergencyContactDetails
                                        </a>
                                    </li>
                                }
                                @if (Common.HasPermission(Constants.HC_Permission_Email_Signature))
                                {
                                    <li id="s3" class="tab-pane" style="margin-right: 10px;">
                                        <a id="addEmployee_EmailSignature" href="#tab_EmailSignature" data-toggle="tab">
                                            Email Signature
                                        </a>
                                    </li>
                                }
                                <li id="s4" class="tab-pane" style="margin-right: 10px;">
                                    <a id="addEmployee_OverTimePay" href="#tab_EmployeeOverTimePay" data-toggle="tab">
                                        Employee Blilling Hours
                                    </a>
                                </li>
                            </ul>
                        </div>
                        <div class="tab-content col-lg-12 col-md-12 col-sm-12">
                            <div class="tab-pane active" id="tab_EmployeeInformation" style="min-height: 350px;">
                                @if (isEditMode)
                                {
                                    <div class="identity-card-container">
                                        <div class="card-block">
                                            <div class="card-header">{{EmployeeModel.Organization.SiteName}}</div>
                                            <div class="card-content">
                                                <div class="card-content-left">
                                                    <div class="profile-image">
                                                        @if (Model.Employee.ProfileImagePath != null)
                                                        {
                                                            <img ng-src="{{EmployeeModel.Employee.ProfileImagePath}}" />
                                                        }
                                                        @if (Model.Employee.ProfileImagePath == null)
                                                        {
                                                            <img src="~/Assets/images/NoImage.png" style="height:100px" />
                                                        }
                                                    </div>
                                                    <div class="content-text">
                                                        <p class="big-name"><strong>{{EmployeeModel.Employee.EmployeeName}}</strong><br />{{EmployeeModel.Employee.DesignationName}}</p>
                                                        <p class="small-text"><strong>EMP Id :</strong> &nbsp;{{EmployeeModel.Employee.EmployeeUniqueID}}</p>
                                                        @*<p class="small-text"><strong>NPI # No.</strong> <br />{{EmployeeModel.Employee.HHA_NPI_ID}}</p>*@
                                                    </div>
                                                </div>
                                                <div class="card-content-right">
                                                    <div class="activated-div"><img data-ng-src="{{EmployeeModel.Organization.SiteLogo}}" /></div>
                                                    <div style="height:15px"></div>
                                                    <div class="signature-block"><img data-ng-src="{{EmployeeModel.Employee.TempSignaturePath}}" /><br /><p>Signatue</p></div>
                                                </div>
                                            </div>
                                            <div class="card-footer">
                                                Powered by <img src="~/Assets/images/marketing_logo.png" /> Myezcare LLC
                                            </div>

                                        </div>
                                        <div class="print-btn"><a href="@Constants.HC_GenerateEmployeeIDCardPdf{{EmployeeModel.Employee.EncryptedEmployeeID}}" class="print">Print</a></div>
                                    </div>
                                }
                                <div class="form-group row">
                                    <div class="col-sm-12">
                                        <div class="fscontainer">
                                            <div class="legend">
                                                @Resource.EmployeeInformation
                                            </div>
                                            <div class="employeeInfo">
                                                <div class="col-md-12">
                                                    <div class="row">
                                                        <div class="col-md-3 col-sm-6">
                                                            <div class="field">
                                                                <label class="control-label">@Resource.Designation</label>
                                                                <div class="field-box">
                                                                    @Html.DropDownListFor(p => p.Employee.Designation, Model.DesignationList.ToSelectList(Model.Employee.Designation.ToString() ?? "", "Name", "Value", Resource.SelectDesignation, ""),
     new
     {
         data_ng_model = "EmployeeModel.Employee.Designation",
         data_ng_init = "EmployeeModel.Employee.Designation=EmployeeModel.Employee.Designation==''?null:EmployeeModel.Employee.Designation.toString()",
         @class = "form-control input-sm"
     })
                                                                    @Html.ValidationMessageFor(q => q.Employee.Designation)
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-3 col-sm-6">
                                                            <div class="field">
                                                                <label class="control-label required">@Resource.EmployeeID</label>
                                                                <div class="field-box">
                                                                    @Html.TextBoxFor(q => q.Employee.EmployeeUniqueID, new { @class = "form-control input-sm", data_ng_model = "EmployeeModel.Employee.EmployeeUniqueID", placeholder = @Resource.EmployeeID, maxlength = 100 })
                                                                    @Html.ValidationMessageFor(q => q.Employee.EmployeeUniqueID)
                                                                </div>
                                                            </div>
                                                        </div>

                                                        <div class="clearfix visible-sm">
                                                            <!-- clearfix sm cols every 2 -->
                                                        </div>

                                                        <div class="col-md-3 col-sm-6">
                                                            <div class="field">
                                                                <label class="control-label required">@Resource.FirstName</label>
                                                                <div class="field-box">
