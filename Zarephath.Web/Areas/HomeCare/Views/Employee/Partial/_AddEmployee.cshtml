                                                            <div class="field">
                                                                <label class="control-label required">@Resource.Address</label>
                                                                <div class="field-box">
                                                                    @Html.TextBoxFor(q => q.Employee.Address,
                     new
                     {
                         @class = "form-control input-sm address-autocomplete requiredStrip",
                         data_ng_model = "EmployeeModel.Employee.Address",
                         placeholder = @Resource.Address,
                         maxlength = 99,
                     })
                                                                </div>
                                                            </div>
                                                        </div>

                                                        <div class="clearfix">
                                                            <!-- clearfix all breakpoints -->
                                                        </div>

                                                        <div class="col-md-3 col-sm-6">
                                                            <div class="field">
                                                                <label class="control-label">@Resource.ApartmentNo</label>
                                                                <div class="field-box">
                                                                    @Html.TextBoxFor(q => q.Employee.ApartmentNo,
                     new
                     {
                         @class = "form-control input-sm",
                         data_ng_model = "EmployeeModel.Employee.ApartmentNo",
                         placeholder = @Resource.ApartmentNo,
                         maxlength = 50,
                     })
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-3 col-sm-6">
                                                            <div class="field">
                                                                <label class="control-label required">@Resource.City</label>
                                                                <div class="field-box">
