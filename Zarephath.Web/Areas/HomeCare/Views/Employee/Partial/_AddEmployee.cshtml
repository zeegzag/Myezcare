                         data_ng_model = "EmployeeModel.Employee.StateCode",
                         data_ng_init = "EmployeeModel.Employee.StateCode=EmployeeModel.Employee.StateCode==''?null:EmployeeModel.Employee.StateCode",
                         @class = "form-control input-sm state-autocomplete requiredStrip",
                     })
                                                                    @Html.ValidationMessageFor(q => q.Employee.StateCode)
                                                                </div>
                                                            </div>
                                                        </div>

                                                        <div class="clearfix">
                                                            <!-- clearfix all breakpoints -->
