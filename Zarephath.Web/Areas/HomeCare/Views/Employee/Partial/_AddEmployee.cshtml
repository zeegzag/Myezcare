                                                                    @Html.ValidationMessageFor(q => q.Employee.MiddleName)
                                                                </div>
                                                            </div>
                                                        </div>

                                                        <div class="clearfix">
                                                            <!-- clearfix all breakpoints -->
                                                        </div>

                                                        <div class="col-md-3 col-sm-6">
