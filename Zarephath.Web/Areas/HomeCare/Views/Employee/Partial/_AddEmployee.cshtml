                                                                   @class = "form-control input-sm zipcode-autocomplete requiredStrip",
                                                                   data_ng_model = "EmployeeModel.Employee.ZipCode",
                                                                   maxlength = Resource.ZipCodemaxlength,
                                                                   placeholder = Resource.ZipCodePlaceholder,
                                                                   dir_valid_number = ""
                                                               })
                                                                    @Html.ValidationMessageFor(q => q.Employee.ZipCode)
