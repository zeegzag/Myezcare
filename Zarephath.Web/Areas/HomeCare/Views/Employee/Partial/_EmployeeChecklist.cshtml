                                               data-uniform-value="EmployeeChecklistModel.DriverLicense"
                                               data-ng-model="EmployeeChecklistModel.DriverLicense"
                                               data-ng-change="CheckboxChange('Identification', 'DriverLicense')"
                                               data-ng-checked="EmployeeChecklistModel.DriverLicense"
                                               data-ng-true-value="1" />
