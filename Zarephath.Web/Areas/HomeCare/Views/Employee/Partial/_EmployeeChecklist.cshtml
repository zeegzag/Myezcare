@model HC_AddEmployeeModel
@Html.Hidden("hdnEmpChecklistModel", Common.SerializeObject(Model.EmployeeChecklistModel))
<form class="form-horizontal" id="frmEmployeeChecklist">  
    <div class="checklistContent">
        <div class="row">
            <div class="col-sm-12">
                <h5><strong>@Resource.ChecklistTitle</strong></h5>
                <hr />
            </div>

            <div class="col-sm-12 margin-bottom-30 clearfix">
                <div class="row margin-bottom-10">
                    <div class="col-sm-3">
                        <label>@Resource.Name</label>
                        <div class="">
                            @Html.TextBox("EmployeeChecklistModelEmployeeName", Model.Employee.EmployeeName, new { @class = "form-control input-sm", disabled = "disabled", data_ng_model = "EmployeeModel.Employee.EmployeeName", placeholder = @Resource.Name, maxlength = 200 })
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <label>@Resource.Email</label>
                        <div class="">
                            @Html.TextBox("EmployeeChecklistModelEmail", Model.Employee.Email, new { @class = "form-control input-sm", disabled = "disabled", data_ng_model = "EmployeeModel.Employee.Email", placeholder = @Resource.Email, maxlength = 256 })
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <label>@Resource.Date</label>
                        <div class="">
                            <div class="input-group date date-picker pull-right"
                                 datepicker="EmployeeChecklistModel.SubmittedDate" ng-dateval="EmployeeChecklistModel.SubmittedDate">
                                @Html.TextBoxFor(m => m.EmployeeChecklistModel.SubmittedDate, new
                        {
                            @class = "form-control input-sm dateInputMask",
                            maxlength = "50",
                            placeholder = Resource.DateFormat,
                            disabled = "disabled"
                        })
                                @Html.ValidationMessageFor(m => m.EmployeeChecklistModel.SubmittedDate)
                                <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <label>@Resource.Address</label>
                        <div class="">
                            @Html.TextBox("EmployeeChecklistModelAddress", Model.Employee.Address, new { @class = "form-control input-sm", disabled = "disabled", data_ng_model = "EmployeeModel.Employee.Address", placeholder = @Resource.Address, maxlength = 200 })
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-3">
                        <label>@Resource.Cell</label>
                        <div class="">
                            @Html.TextBox("EmployeeChecklistModelMobileNumber", Model.Employee.MobileNumber, new { @class = "form-control input-sm", disabled = "disabled", data_ng_model = "EmployeeModel.Employee.MobileNumber", placeholder = @Resource.Cell, maxlength = 50 })
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <label>@Resource.HomePhone</label>
                        <div class="">
                            @Html.TextBox("EmployeeChecklistModelHomePhone", Model.Employee.PhoneHome, new { @class = "form-control input-sm", disabled = "disabled", data_ng_model = "EmployeeModel.Employee.PhoneHome", placeholder = @Resource.HomePhone, maxlength = 50 })
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-sm-12 clearfix">
                <div class="table-responsive">
                    <table class="table table-bordered employee-checklist">
                        <thead>
                            <tr class="bg-grey-salsa">
                                <td>@Resource.IdentificationRequirements</td>
                                <td>@Resource.ExpirationDate</td>
                                <td>@Resource.CompletionDate</td>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>
                                    <label class="mt-checkbox mt-checkbox-outline">
