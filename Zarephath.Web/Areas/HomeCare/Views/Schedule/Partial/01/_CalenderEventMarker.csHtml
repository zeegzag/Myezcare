<div calender-dropper="DataModel">
    <div class="event-container webpopUiCls height-39px" data-ng-if="DataModel.event.scheduleModel.UnAllocated==false || (DataModel.event.scheduleModel.UsedInScheduling==false || DataModel.event.scheduleModel.IsDenied==true)"
         common-detail-popover="DataModel" ng-template-selector="#eventdetailpopover" popover-width="400" popover-trigger="click" popover-closeable="true" pop-over-functions="DataModel.event.popOverObj">
        <span  class='fc-name hc-fc-name'>
            <span data-ng-bind-html="DataModel.event.scheduleModel.StartDate | twodatetimeformathtml: DataModel.event.scheduleModel.EndDate"></span>
            <span class="clearfix">{{DataModel.event.scheduleModel.EmployeeName ? DataModel.event.scheduleModel.EmployeeName : 
                (DataModel.event.scheduleModel.UsedInScheduling && !DataModel.event.scheduleModel.IsDenied) ? '@Resource.NotScheduled' :'@Resource.DoNotSchedule' }}</span>
        </span>
    </div>


    <div class="event-container webpopUiCls height-39px" data-ng-show="DataModel.event.scheduleModel.UnAllocated && (DataModel.event.scheduleModel.UsedInScheduling==true && DataModel.event.scheduleModel.IsDenied==false)"
         data-ng-click="( DataModel.event.OpenEmpRefSchModal(DataModel.event.scheduleModel.ReferralID,DataModel.event,DataModel.event.scheduleModel.CareTypeId))  ||
        (DataModel.event.isPastEvent)
        ">
        <span  class='fc-name hc-fc-name'>
            <span data-ng-bind-html="DataModel.event.scheduleModel.StartDate | twodatetimeformathtml: DataModel.event.scheduleModel.EndDate"></span>
            <span class="plus clearfix">{{DataModel.event.scheduleModel.EmployeeName ? DataModel.event.scheduleModel.EmployeeName : 
                (DataModel.event.scheduleModel.UsedInScheduling && !DataModel.event.scheduleModel.IsDenied) ? '+' :'+' }}</span>
        </span>
    </div>
   
</div>
