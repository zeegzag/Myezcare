                            <table class="table table-bordered table-striped table-condensed flip-content">
                                <thead class="flip-content">
                                    <tr>
                                        <th class="width-400px text-align-center">
                                            <a class="anchor_color" href="javascript:void(0);">ID</a>
                                        </th>
                                        <th class="width-400px text-align-center">
                                            <a class="anchor_color" href="javascript:void(0);">CREATED BY</a>
                                        </th>
                                        <th class="width-400px text-align-center">CREATED DATE</th>
                                        <th class="width-400px text-align-center">ACTION</th>
                                    </tr>
                                </thead>
                                <tbody data-ng-repeat="item in DMAS97ABList">
                                    <tr>
                                        <td class="text-align-center">{{item.Dmas97ID}}</td>
                                        <td class="text-align-center">{{item.EmployeeName}}</td>
                                        <td class="text-align-center">{{item.CreatedDate}}</td>
                                        <td class="text-align-center">
                                            @if (Common.HasPermission(Constants.HC_Permission_PatientIntake_DMAS97_Add))
                                            {
                                                <a data-ng-click="AddDmas97(item)" class="btn btn-xs blue" title="Edit"><i class="fa fa-edit"></i></a>
                                                <a data-ng-click="DeleteDmas97Ab(item)" class="btn btn-xs red" title="Delete"><i class="fa fa-trash"></i></a>
                                            }
                                            @if (Common.HasPermission(Constants.HC_Permission_PatientIntake_DMAS97_Delete))
                                            {
                                                <a data-ng-click="DeleteDmas97Ab(item)" class="btn btn-xs red" title="Delete"><i class="fa fa-trash"></i></a>
                                            }
                                            <a href="@Constants.Generate_Dmas97_Pdf{{item.Dmas97ID}}" class="btn btn-xs green-jungle" title="Print Pdf"><i class="fa fa-print"></i></a>
