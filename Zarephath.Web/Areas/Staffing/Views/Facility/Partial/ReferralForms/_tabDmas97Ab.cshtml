                    <button class="btn blue btn-sm grey border-grey-cascade" data-ng-click="Refresh()">
                        <i class="fa fa-refresh"></i>&nbsp;&nbsp;@Resource.Refresh
                    </button>
                </div>

                @if (Common.HasPermission(Constants.HC_Permission_PatientIntake_DMAS97_Add))
                {
                    <div class="btn-group padding-left-15 pull-right">
                        <button type="button" class="btn btn-primary btn-sm pull-right" data-ng-click="AddDmas97()"><i class="fa fa-plus">&nbsp;&nbsp;</i>DMAS-97</button>
                    </div>
                }

            </div>
            <div class="space20"></div>
        </div>

