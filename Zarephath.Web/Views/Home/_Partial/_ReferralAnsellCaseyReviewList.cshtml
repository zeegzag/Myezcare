<div id="ReferralAnsellCaseyReviewList" class="row" data-ng-show="ReferralAnsellCaseyReviewList.length > 0">
    <div class="col-md-12">
        <div class="portlet box yellow-lemon">

            <div class="portlet-title">
                <div class="caption header-font-size-14 uppercase">
                    @Resource.AnsellCaseyAssessmentReview
                    <div class="pull-right padding-left-25" data-ng-show="ReferralAnsellCaseyReviewAjaxStart">
                        <img src="~/Assets/images/lazyload.gif" />
                    </div>
                </div>
                <div class="tools">
                    <a href="javascript:;" class="collapse"></a>
                    <a href="javascript:;" class="fullscreen"></a>
                </div>
            </div>

            <div class="portlet-body table_font-size-12 ">
                <div class="row">
                    <div class="col-sm-12 form-group  padding-right-15" data-ng-if="ReferralAnsellCaseyReviewList.length > 0">
                        <div class="pull-right padding-right-5">
                            <dir-pagination-controls boundary-links="true" on-page-change="ReferralAnsellCaseyReviewListPager.pageChanged(newPageNumber)" pagination-id="ReferralAnsellCaseyList"></dir-pagination-controls>
                        </div>
                    </div>
                </div>
                @*<div class="table-responsive">*@
                <div class="row">
                    <div class="col-sm-12">
                        <div class="portlet-body" style="display: block;">
                            <div class="table-responsive">
                                <form id="frmReferralinterMessageList">
                                    <table class="table table-bordered table-striped table-condensed flip-content">
                                        <thead>
                                            <tr>
                                                <th>
                                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="ReferralAnsellCaseyReviewListPager.sortColumn('AHCCCSID')">@Resource.AHCCCSID</a>
                                                    <span class="sortorder" data-ng-show="ReferralAnsellCaseyReviewListPager.sortIndex === 'AHCCCSID'" data-ng-class="{reverse:ReferralAnsellCaseyReviewListPager.reverse}"></span>
                                                </th>
                                                <th>
                                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="ReferralAnsellCaseyReviewListPager.sortColumn('CISNumber')">@Resource.CISLabel</a>
                                                    <span class="sortorder" data-ng-show="ReferralAnsellCaseyReviewListPager.sortIndex === 'CISNumber'" data-ng-class="{reverse:ReferralAnsellCaseyReviewListPager.reverse}"></span>
                                                </th>
                                                <th>
                                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="ReferralAnsellCaseyReviewListPager.sortColumn('ClientName')">@Resource.ClientLabel</a>
                                                    <span class="sortorder" data-ng-show="ReferralAnsellCaseyReviewListPager.sortIndex === 'ClientName'" data-ng-class="{reverse:ReferralAnsellCaseyReviewListPager.reverse}"></span>
                                                </th>
                                                <th>
                                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="ReferralAnsellCaseyReviewListPager.sortColumn('Gender')">@Resource.Sex</a>
                                                    <span class="sortorder" data-ng-show="ReferralAnsellCaseyReviewListPager.sortIndex === 'Gender'" data-ng-class="{reverse:ReferralAnsellCaseyReviewListPager.reverse}"></span>
                                                </th>
                                                <th>
                                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="ReferralAnsellCaseyReviewListPager.sortColumn('Age')">@Resource.Age</a>
                                                    <span class="sortorder" data-ng-show="ReferralAnsellCaseyReviewListPager.sortIndex === 'Age'" data-ng-class="{reverse:ReferralAnsellCaseyReviewListPager.reverse}"></span>
                                                </th>
                                                <th>
                                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="ReferralAnsellCaseyReviewListPager.sortColumn('CreatedByName')">@Resource.AddedBy</a>
                                                    <span class="sortorder" data-ng-show="ReferralAnsellCaseyReviewListPager.sortIndex === 'CreatedByName'" data-ng-class="{reverse:ReferralAnsellCaseyReviewListPager.reverse}"></span>
                                                </th>
                                                <th>
                                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="ReferralAnsellCaseyReviewListPager.sortColumn('CreatedDate')">@Resource.AddedDate</a>
                                                    <span class="sortorder" data-ng-show="ReferralAnsellCaseyReviewListPager.sortIndex === 'CreatedDate'" data-ng-class="{reverse:ReferralAnsellCaseyReviewListPager.reverse}"></span>
                                                </th>
                                                <th>
                                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="ReferralAnsellCaseyReviewListPager.sortColumn('AssessmentDate')">@Resource.ReviewDate</a>
                                                    <span class="sortorder" data-ng-show="ReferralAnsellCaseyReviewListPager.sortIndex === 'AssessmentDate'" data-ng-class="{reverse:ReferralAnsellCaseyReviewListPager.reverse}"></span>
                                                </th>
                                                <th>
                                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="ReferralAnsellCaseyReviewListPager.sortColumn('OverAllAverage')">@Resource.Average</a>
                                                    <span class="sortorder" data-ng-show="ReferralAnsellCaseyReviewListPager.sortIndex === 'OverAllAverage'" data-ng-class="{reverse:ReferralAnsellCaseyReviewListPager.reverse}"></span>
                                                </th>
                                                <th>
                                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="ReferralAnsellCaseyReviewListPager.sortColumn('AssignedByName')">@Resource.AssignedBy</a>
                                                    <span class="sortorder" data-ng-show="ReferralAnsellCaseyReviewListPager.sortIndex === 'AssignedByName'" data-ng-class="{reverse:ReferralAnsellCaseyReviewListPager.reverse}"></span>
                                                </th>

                                                
                                                @if (Common.HasPermission(Constants.Permission_ReviewMeasurement_AnsellCaseyReview_AddUpdate))
                                                {
                                                    <th></th>
                                                }
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr dir-paginate="item in ReferralAnsellCaseyReviewList | itemsPerPage: ReferralAnsellCaseyReviewListPager.pageSize" total-items="ReferralAnsellCaseyReviewListPager.totalRecords" current-page="ReferralAnsellCaseyReviewListPager.currentPage" pagination-id="ReferralAnsellCaseyList">

                                                <td>
                                                    <span data-ng-bind="item.AHCCCSID"></span>
                                                </td>
                                                <td>
                                                    <span data-ng-bind="item.CISNumber? item.CISNumber:'@Resource.NA'"></span>
                                                </td>
                                                <td>
                                                    <span data-ng-bind="item.LastName +', '+ item.FirstName"></span>
                                                </td>
                                                <td>
                                                    <span data-ng-bind="item.Gender"></span>
                                                </td>
                                                <td>
                                                    <span data-ng-bind="item.Age"></span>
                                                </td>
                                                <td><span data-ng-bind="item.CreatedByName"></span></td>
                                                <td>
                                                    <span data-ng-bind="item.CreatedDate | dateformat | date: 'MM/dd/yyyy'"></span>
                                                </td>

                                                <td>
                                                    <span data-ng-bind="item.AssessmentDate | dateformat | date: 'MM/dd/yyyy'"></span>
                                                </td>
                                                <td>
                                                    <span>{{item.OverAllAverage  | number : 2}}</span>
                                                </td>

                                                 <td>
                                                    <span>{{item.AssignedByName}}</span>
                                                </td>

                                                @if (Common.HasPermission(Constants.Permission_ReviewMeasurement_AnsellCaseyReview_AddUpdate))
                                                {
                                                    <td>
                                                        <div>
                                                            <a href="#" target="_blank" data-ng-attr-href="{{ ReferralURL + item.EncryptedReferralID +'/#rm_AnsellCaseyReview'}}" class="btn btn-xs blue">@Resource.AccessClient</a>
                                                        </div>
                                                    </td>
                                                }
                                            </tr>
                                        </tbody>
                                    </table>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                @*</div>*@
                <div class="row">
                    <div class="col-sm-12" data-ng-if="ReferralAnsellCaseyReviewList.length > 0">
                        <div class="col-sm-6 pull-left no-left-padding"
                            data-ng-hide="ReferralAnsellCaseyReviewListPager.currentPage==1 && (ReferralAnsellCaseyReviewListPager.currentPageSize < ReferralAnsellCaseyReviewListPager.pageSize)">
                            @Resource.Showing <span data-ng-bind="(ReferralAnsellCaseyReviewListPager.pageSize * ReferralAnsellCaseyReviewListPager.currentPage) -ReferralAnsellCaseyReviewListPager.pageSize  + 1"></span>
                            &nbsp;@Resource.To <span data-ng-bind="(ReferralAnsellCaseyReviewListPager.pageSize == ReferralAnsellCaseyReviewListPager.currentPageSize) ? (ReferralAnsellCaseyReviewListPager.pageSize * ReferralAnsellCaseyReviewListPager.currentPage) :(ReferralAnsellCaseyReviewListPager.pageSize * ReferralAnsellCaseyReviewListPager.currentPage) - ReferralAnsellCaseyReviewListPager.pageSize  + ReferralAnsellCaseyReviewListPager.currentPageSize"></span>&nbsp;@Resource.Of &nbsp;<span data-ng-bind="ReferralAnsellCaseyReviewListPager.totalRecords"></span>&nbsp;@Resource.Records
                        </div>
                        <div class="pull-right">
                            <dir-pagination-controls boundary-links="true" on-page-change="ReferralAnsellCaseyReviewListPager.pageChanged(newPageNumber)" pagination-id="ReferralResolvedInternalMessageList"></dir-pagination-controls>
                        </div>
                    </div>
                </div>
                <br class="clearfix" />
            </div>
        </div>
    </div>

</div>
