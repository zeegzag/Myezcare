@model ScheduleMasterModel
<div id="ScheduleNotificationLog-modal" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h3 class="modal-title">@Resource.ScheduleNotificationLogDetails</h3>
            </div>
            <div class="modal-body">
                <table class="table table-bordered table-striped table-condensed flip-content table-checkable" data-ng-if="ScheduleNotificationDetails.length>0">
                    <thead class="flip-content">
                    <tr>
                        <th>@Resource.Type</th>
                        <th>@Resource.SentBy</th>
                        <th>@Resource.SentDate</th>
                        <th>@Resource.ToEmail</th>
                        <th>@Resource.ToPhone</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr data-ng-repeat="model in ScheduleNotificationDetails">
                        <td><span class="badge badge-info">{{model.NotificationType}}</span> </td>
                        <td>{{model.CreatedBy}}</td>
                        <td>{{model.CreatedDate | dateformat}}</td>
                        <td>{{model.ToEmailAddress}}</td>
                        <td>{{model.ToPhone}}</td>
                    </tr>
                    </tbody>
                </table>
                
                <div data-ng-if="ScheduleNotificationDetails.length == 0">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="alert alert-danger" data-ng-show="!AjaxStart">
                                        @Html.Raw(Resource.NoRecordsFound)
                                    </div>
                                    <div class="top pagination-sm" data-ng-show="AjaxStart">
                                        @Resource.Loading
                                        <img src="~/Assets/images/lazyload.gif" />
                                    </div>
                                </div>
                            </div>
                        </div>
            </div>
            <div class="modal-footer">
                <a class="btn btn-success bg-green-jungle"  data-dismiss="modal">@Resource.Okay</a>
            </div>
        </div>

    </div>
</div>
