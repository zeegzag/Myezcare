@model SetNoteListModel
@{
    ViewBag.Title = Resource.NoteReview;
    var addClearFixToPartial = "";
    var addClearFixToMain = "";
    Layout = Model.IsPartial ? null : "~/Views/Shared/_Layout.cshtml";
    if (!Model.IsPartial)
    {
        ViewBag.SelectedMenuItem = Resource.SMI_AddBillingNoteGC;
        addClearFixToMain = "clear-both";
    }
    else
    {
        addClearFixToPartial = "clear-both";

    }
}


@Html.Hidden("hdnSetNoteListPage", Common.SerializeObject(Model))

<div data-ng-controller="NoteController">
    <!--#region Page Header -->
    @if (!Model.IsPartial)
    {
        <div class="page-bar">
            <h3 class="page-title">@Resource.NoteReview

                <div class="btn-group pull-right btn-group-xs btn-group-devided">
                    <button type="button" class="btn collapsed grey border-grey-cascade btn-xs" data-toggle="collapse" data-target="#filter-panel">
                        <i class="fa fa-sliders" aria-hidden="true"></i>&nbsp; @Resource.AdditionalFilters
                    </button>


                    <a href="#" data-ng-click="OpenAddNoteModel()" class="btn blue  border-blue-sharp" data-toggle="modal">
                        <i class="fa fa-plus">&nbsp;&nbsp;</i>@Resource.AddNote
                    </a>
                    @if (!Model.IsPartial && Common.HasPermission(Constants.Permission_GroupNote))
                    {
                        <a href="@Constants.AddGroupNote" target="_blank" class="btn blue  border-blue-sharp">
                            <i class="fa fa-plus">&nbsp;&nbsp;</i>@Resource.AddGroupNote
                        </a>
                    }
                </div>


            </h3>
        </div>
    }
    <!--#endregion-->

    <!--#region Upper Search Panel  -->
    <div class="row">
        <div class="col-sm-12 table-responsive overflow-i">
            <div>
                <div>
                    <div class="col-md-12 col-sm-12 no-padding ">
                        <form id="frm_clientNote">
                            <input id="hideErrorContent" type="text" class="hideErrorContent">
                            <div class=" form-group col-lg-2 col-md-4 col-sm-4 col-xs-6">
                                <label class="control-label">@Resource.ServiceFromDate</label>
                                <div class="input-group date" datepicker="TempSearchNoteModel.ServiceDateStart" ng-dateval="TempSearchNoteModel.ServiceDateStart" data-ng-maxdate="TempSearchNoteModel.ServiceDateEnd">
                                    @Html.TextBoxFor(q => q.SearchNoteListModel.ServiceDateStart,
                                new
                                {
                                    @class = "form-control input-sm dateInputMask",
                                    placeholder = Constants.GlobalDateFormat,
                                    @maxlength = "10"
                                })
                                    @Html.ValidationMessageFor(q => q.SearchNoteListModel.ServiceDateStart)
                                    <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                </div>
                            </div>
                            <div class="form-group  col-lg-2 col-md-4 col-sm-4 col-xs-6">
                                <label class="control-label">@Resource.ServiceToDate</label>
                                <div class="input-group date" datepicker="TempSearchNoteModel.ServiceDateEnd" ng-dateval="TempSearchNoteModel.ServiceDateEnd" data-ng-mindate="TempSearchNoteModel.ServiceDateStart">
                                    @Html.TextBoxFor(q => q.SearchNoteListModel.ServiceDateEnd,
                                new
                                {
                                    @class = "form-control input-sm dateInputMask",
                                    placeholder = Constants.GlobalDateFormat,
                                    @maxlength = "10"
                                })
                                    @Html.ValidationMessageFor(q => q.SearchNoteListModel.ServiceDateEnd)
                                    <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                </div>
                            </div>
                            <div class="form-group  col-lg-1 col-md-4 col-sm-4 col-xs-6">
                                <label class="control-label">@Resource.Facility
                                    <i class="fa fa-question-circle font-size-16" data-ng-tooltip="@Resource.RenderingProvider" data-ng-custom-class="tooltip-extra-z-index"></i>
                                </label>
                                @Html.DropDownListFor(p => p.SearchNoteListModel.BillingProviderID, Model.Facilities.ToSelectList(Model.SearchNoteListModel.BillingProviderID.ToString() ?? "", "Name", "Value", @Resource.Select, ""),
                                new
                                {
                                    convert_to_number = "",
                                    data_val = false,
                                    id = "BillingProvider",
                                    data_ng_model = "TempSearchNoteModel.BillingProviderID",
                                    data_ng_init = "TempSearchNoteModel.BillingProviderID=TempSearchNoteModel.BillingProviderID==''?null:TempSearchNoteModel.BillingProviderID.toString()",
                                    @class = "form-control input-sm",
                                })
                            </div>

                            <div class="form-group  col-lg-1 col-md-4 col-sm-4 col-xs-6">
                                <label class="control-label">@Resource.Region</label>
                                @Html.DropDownListFor(p => p.SearchNoteListModel.RegionID, Model.Regions.ToSelectList(Model.SearchNoteListModel.RegionID.ToString() ?? "", "Name", "Value", @Resource.Select, ""),
                                new
                                {
                                    convert_to_number = "",
                                    data_val = false,
                                    id = "RegionID",
                                    data_ng_model = "TempSearchNoteModel.RegionID",
                                    data_ng_init = "TempSearchNoteModel.RegionID=TempSearchNoteModel.RegionID==''?null:TempSearchNoteModel.RegionID.toString()",
                                    @class = "form-control input-sm",
                                })
                            </div>

                            <div class="additionalFilters filter-panel collapse" id="filter-panel">
                                @if (!Model.IsPartial)
                                {
                                    <div class="form-group col-lg-2 col-md-4 col-sm-4 col-xs-6">
                                        <label class="control-label">@Resource.ClientLabel</label>
                                        <div class="">
                                            <input type="text"
                                           token-input
                                           ng-val="TempSearchNoteModel.ReferralID"
                                           textfield='Name'
                                           valuefield='ReferralID'
                                           searchurl="{{GetReferralsURL}}"
                                           placeholder="@Resource.SearchClientInformation"
                                           tokenlimit='1'
                                           onresultsformatter='ReferralDetailResultsFormatter'
                                           ontokenformatter="ReferralTokenFormatter"
                                           autoSelectFirstResult="false"
                                           minChars="1"
                                           tokeninputobj="ReferralTokenObj"
                                           id="ReferralIDTkn"
                                           customclass="form-control input-sm"
                                           name="TempSearchNoteModel.ReferralID"/>
                                        </div>
                                    </div>
                                }

@*<div class="form-group col-lg-2 col-md-4 col-sm-4 col-xs-6 display-none">
                                    <label class="control-label">@Resource.ServiceCode</label>
                                    <div>
                                        <input type="text" token-input-local
                                               ng-val="TempSearchNoteModel.ServiceCodeID"
                                               textfield='ServiceCode'
                                               valuefield='ServiceCodeID'
                                               searchurl="{{GetBatchURL}}"
                                               local-data="NoteModel.ServiceCodes | unique:'ServiceCodeID'"
                                               placeholder="@Resource.EnterServiceCode"
                                               tokenlimit='1'
                                               onresultsformatter='ServiceCodeResultsFormatter'
                                               ontokenformatter="ServiceCodeTokenFormatter"
                                               id="SearchServiceCodeToken"
                                               customclass="form-control input-sm "/>
                                    </div>

                                </div>*@

                                <div class="form-group col-lg-2 col-md-4 col-sm-4 col-xs-6">
                                    <label class="control-label">@Resource.ServiceCode</label>
                                    <div class="transportation-dropdown">
                                        <select class="form-control input-sm selectpicker" data-buttonstyle="btn-sm btn-default" multiple="multiple"
                                            title="@Resource.ServiceCode" data-val="false" id="ServiceCodeID"
                                            selectpicker="TempSearchNoteModel.ServiceCodeIDs" name="TempSearchNoteModel.ServiceCodeID">
                                            <option data-ng-repeat="option in NoteModel.ServiceCodes  | unique:'ServiceCodeID'" value="{{option.ServiceCodeID}}">{{option.ServiceCode +' : '+ option.ServiceName}}</option>
                                        </select>
                                    </div>
                                </div>


                                <div class="form-group col-lg-1 col-md-4 col-sm-4 col-xs-6">
                                    <label class="control-label">@Resource.Type</label>
                                    @Html.DropDownListFor(p => p.SearchNoteListModel.ServiceCodeTypeID, Model.ServiceCodeTypes.ToSelectList(Model.SearchNoteListModel.ServiceCodeTypeID.ToString() ?? "", "ServiceCodeTypeName", "ServiceCodeTypeID", "All", "-1"),
                                        new
                                        {
                                            convert_to_number = "",
                                            id = "ServiceCodeTypeID",
                                            data_ng_model = "TempSearchNoteModel.ServiceCodeTypeID",
                                            data_ng_init = "TempSearchNoteModel.ServiceCodeTypeID=TempSearchNoteModel.ServiceCodeTypeID==''?null:TempSearchNoteModel.ServiceCodeTypeID",
                                            @class = "form-control input-sm",
                                            data_val = false
                                        })

                                </div>
                                <div class="form-group col-lg-1 col-md-4 col-sm-4 col-xs-6">
                                    <label class="control-label">@Resource.Payor</label>
                                    @Html.DropDownListFor(p => p.SearchNoteListModel.PayorID, Model.PayorList.ToSelectList(Model.SearchNoteListModel.PayorID.ToString() ?? "", "Name", "Value", "All", "-1"),
                                        new
                                        {
                                            convert_to_number = "",
                                            id = "PayorID",
                                            data_ng_model = "TempSearchNoteModel.PayorID",
                                            data_ng_init = "TempSearchNoteModel.PayorID=TempSearchNoteModel.PayorID==''?null:TempSearchNoteModel.PayorID",
                                            @class = "form-control input-sm",
                                            data_val = false
                                        })

                                </div>
                                <div class="form-group col-lg-1 col-md-4 col-sm-4 col-xs-6 @addClearFixToMain">
                                    <label class="control-label">@Resource.BatchNumberLabel</label>
                                    <div>
                                        @Html.TextBoxFor(q => q.SearchNoteListModel.BatchID, new { @class = "form-control input-sm", data_ng_model = "TempSearchNoteModel.BatchID", placeholder = @Resource.BatchNumberLabel, maxlength = 6 })
                                    </div>
                                </div>
                                <div class="form-group col-lg-1 col-md-4 col-sm-4 col-xs-6">
                                    <label class="control-label">@Resource.NoteNew</label>
                                    <div>
                                        @Html.TextBoxFor(q => q.SearchNoteListModel.NoteID, new { @class = "form-control input-sm", data_ng_model = "TempSearchNoteModel.NoteID", placeholder = @Resource.NoteNew, maxlength = 6 })
                                    </div>
                                </div>


                                <div class="form-group col-lg-1 col-md-4 col-sm-4 col-xs-6">
                                    <label class="control-label">@Resource.NoteKind</label>
                                    @Html.DropDownListFor(p => p.SearchNoteListModel.NoteKind, Model.NoteKinds.ToSelectList(Model.SearchNoteListModel.NoteKind ?? "", "Value", "Value", "All", ""),
                                new
                                {
                                    id = "NoteKind",
                                    data_ng_model = "TempSearchNoteModel.NoteKind",
                                    data_ng_init = "TempSearchNoteModel.NoteKind=TempSearchNoteModel.NoteKind==''?null:TempSearchNoteModel.NoteKind",
                                    @class = "form-control input-sm",
                                    data_val = false
                                })
                                </div>

                                <div class="form-group col-lg-1 col-md-4 col-sm-4 col-xs-6">
                                    <label class="control-label">@Resource.NoteType</label>
                                    @Html.DropDownListFor(p => p.SearchNoteListModel.IsBillable, Model.NoteTypes.ToSelectList(Model.SearchNoteListModel.IsBillable.ToString() ?? "", "Name", "Value"),
                                new
                                {
                                    convert_to_number = "",
                                    id = "IsBillable",
                                    data_ng_model = "TempSearchNoteModel.IsBillable",
                                    data_ng_init = "TempSearchNoteModel.IsBillable=TempSearchNoteModel.IsBillable==''?null:TempSearchNoteModel.IsBillable",
                                    @class = "form-control input-sm",
                                    data_val = false
                                })
                                </div>

                                <div class="form-group col-lg-1 col-md-4 col-sm-4 col-xs-6">
                                    <label class="control-label">@Resource.Completed?</label>
                                    @Html.DropDownListFor(p => p.SearchNoteListModel.IsCompleted, Model.CompletedFilter.ToSelectList(Model.SearchNoteListModel.IsCompleted.ToString() ?? "", "Name", "Value"),
                                new
                                {
                                    convert_to_number = "",
                                    id = "IsCompleted",
                                    data_ng_model = "TempSearchNoteModel.IsCompleted",
                                    data_ng_init = "TempSearchNoteModel.IsCompleted=TempSearchNoteModel.IsCompleted==''?null:TempSearchNoteModel.IsCompleted",
                                    @class = "form-control input-sm",
                                    data_val = false
                                })
                                </div>




                                <div class="form-group col-lg-1 col-md-4 col-sm-4 col-xs-6">
                                    <label class="control-label">@Resource.Department</label>
                                    @Html.DropDownListFor(p => p.SearchNoteListModel.DepartmentID, Model.Departments.ToSelectList(Model.SearchNoteListModel.DepartmentID.ToString() ?? "", "Name", "Value", @Resource.All, ""),
                                new
                                {
                                    convert_to_number = "",
                                    id = "Departments",
                                    data_ng_model = "TempSearchNoteModel.DepartmentID",
                                    data_ng_init = "TempSearchNoteModel.DepartmentID=TempSearchNoteModel.DepartmentID==''?null:TempSearchNoteModel.DepartmentID.toString()",
                                    @class = "form-control input-sm",
                                    data_val = false
                                })
                                </div>


                                <div class="form-group col-lg-2 col-md-4 col-sm-4 col-xs-6">
                                    <label class="control-label">@Resource.CreatedBy</label>

                                    <div class="transportation-dropdown">
                                        <select class="form-control input-sm selectpicker" data-buttonstyle="btn-sm btn-default" multiple="multiple"
                                            title="@Resource.CreatedBy" data-val="false" id="CreatedBy"
                                            selectpicker="TempSearchNoteModel.CreatedByIDs" name="TempSearchNoteModel.CreatedByIDs">
                                            <option data-ng-repeat="option in NoteModel.Employees  | unique:'Value'" value="{{option.Value}}">{{option.Name}}</option>
                                        </select>
                                    </div>


                                    @*@Html.DropDownListFor(p => p.SearchNoteListModel.AssigneeID, Model.Employees.ToSelectList(Model.SearchNoteListModel.AssigneeID.ToString() ?? "", "Name", "Value"),
                                        new
                                        {
                                            convert_to_number = "",
                                            id = "EmployeeID",
                                            data_ng_model = "TempSearchNoteModel.EmployeeID",
                                            data_ng_init = "TempSearchNoteModel.EmployeeID=TempSearchNoteModel.EmployeeID==''?null:TempSearchNoteModel.EmployeeID.toString()",
                                            @class = "form-control input-sm selectpicker",
                                            @data_buttonstyle = "btn-sm btn-default",
                                            @title = @Resource.CreatedBy,
                                            @selectpicker = "TempSearchNoteModel.CreatedByIDs",
                                            multiple = "multiple",
                                            data_val = false
                                        })*@
                                </div>
                                @if (Common.HasPermission(Constants.Permission_NoteList_ViewAll))
                                {
                                    <div class="form-group col-lg-2 col-md-4 col-sm-4 col-xs-6">
                                        <label class="control-label">@Resource.Assignee <small><b>(@Resource.ORConditionApplied)</b></small></label>
                                        @Html.DropDownListFor(p => p.SearchNoteListModel.AssigneeID, Model.Assignees.ToSelectList(Model.SearchNoteListModel.AssigneeID.ToString() ?? "", "Name", "Value", @Resource.AllStaff, ""),
                                             new
                                             {
                                                 convert_to_number = "",
                                                 id = "EmployeeID",
                                                 data_ng_model = "TempSearchNoteModel.AssigneeID",
                                                 data_ng_init = "TempSearchNoteModel.AssigneeID=TempSearchNoteModel.AssigneeID==''?null:TempSearchNoteModel.AssigneeID.toString()",
                                                 @class = "form-control input-sm",
                                                 data_val = false
                                             })
                                    </div>
                                }

                            </div>



                            <div class="form-group col-lg-2 col-md-4 col-sm-4 col-xs-6 padding-top-18">
                                <div class="pull-left col-md-push-1 col-md-11 no-padding">
                                    <div class="col-md-6 no-right-padding">
                                        <button type="submit" class="btn btn-sm btn-primary form-control" data-ng-click="SearchNote()">
                                            @Resource.Search
                                        </button>
                                    </div>
                                    <div class="col-md-6 no-right-padding">
                                        <button class="btn btn-sm grey border-grey-cascade form-control cancel" data-ng-click="ResetSearchFilter()">
                                            @Resource.Reset
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--#endregion-->

    <!--#region Upper Refresh, Action Button & Paging Panel  -->
    <div class="space20"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            @if (Common.HasPermission(Constants.Permission_ReferralDetails_View_AddUpdate))
            {
                <div class="btn-group" data-ng-show="NoteList.length > 0">
                    <span class="btn blue btn-sm grey border-grey-cascade" style="padding: 5px;">
                        <input id="SelectAllNote" class="unicheckbox text-align-center" type="checkbox" data-ng-model="SelectAllCheckbox" data-ng-change="SelectAll()" val="SelectAllCheckbox">
                        <label for="SelectAllNote"></label>
                    </span>
                </div>            
            }
            <div class="btn-group">
                <button class="btn blue btn-sm grey border-grey-cascade" data-ng-click="Refresh()">
                    <i class="fa fa-refresh"></i>&nbsp;&nbsp;@Resource.Refresh
                </button>
            </div>
            <div class="btn-group">
                @Html.DropDownListFor(m => m.SearchNoteListModel.IsDeleted, Model.DeleteFilter.ToSelectList(Convert.ToString(Model.SearchNoteListModel.IsDeleted), "Name", "Value", "", ""),
                    new
                    {
                        id = "IsDeleted",
                        data_ng_model = "TempSearchNoteModel.IsDeleted",
                        data_ng_init = "TempSearchNoteModel.IsDeleted = TempSearchNoteModel.IsDeleted==null?'-1':TempSearchNoteModel.IsDeleted.toString()",
                        @class = "form-control input-sm btn blue btn-sm grey border-grey-cascade",
                        data_ng_change = "SearchNote()"
                    })
            </div>

            @if (Common.HasPermission(Constants.Permission_Snapshot_Review))
            {
                <div class="btn-group">
                    <a class="btn btn-sm  green-dark   border-grey-cascade margin-left-10" target="_blank" href="@Constants.MonthalySummaryList">@Resource.ViewSnapShots</a>
                </div>
            }


            <div class="btn-group" data-ng-if="SelectedNoteIds.length > 0">
                <button type="button" data-ng-click="DeleteNote()" class="btn blue btn-sm grey border-grey-cascade">
                    <i class="fa fa-random margin-right-10"></i>{{TempSearchNoteModel.IsDeleted==0?'@Constants.DisableSelected' :(TempSearchNoteModel.IsDeleted==1?'@Constants.EnableSelected':'@Constants.InverseSelected')}}</button>
            </div>



            <div class="btn-group margin-left-20PR">
                <div class="top pagination-sm" data-ng-show="GetNoteClientListAjaxCall">
                    <b>@Resource.Loading</b>
                    <img src="~/Assets/images/lazyload.gif" />
                </div>
            </div>

            <div class="inline-block pull-right" data-ng-show="NoteClientList.length > 0">
                <div class="pull-right top pagination-sm">
                    <dir-pagination-controls boundary-links="true" max-size="NoteClientListPager.maxSize" on-page-change="NoteClientListPager.pageChanged(newPageNumber)" pagination-id="NoteClientListPagerID"></dir-pagination-controls>
                </div>
            </div>

            <div class="pull-right excel-export">

                <a data-ng-click="ExportNoteList()" title="@Resource.ExportToExcel">
                    <i class="font-green-soft fa fa-file-excel-o" style="font-size: 25px;"></i></a>
            </div>
        </div>
        <div class="space20"></div>
    </div>
    <!--#endregion -->

    <!--#region Note List Group By Client -->
    <div class="row" data-ng-hide="GetNoteClientListAjaxCall">
        <div class="col-sm-12" data-ng-if="NoteClientList.length != 0">
            <div class="portlet-body compact" style="display: block;">
                <div class="table-responsive tblCollpase">
                    <form id="frmReferralTrackingList">
                        <table class=" table table-bordered table-striped table-condensed flip-content">

                            <thead class="">
                                <tr>
                                    <th class="width-25px"></th>
                                    <th>
                                        <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteClientListPager.sortColumn('ReferralName')">@Resource.Name</a>
                                        <span class="sortorder" data-ng-show="NoteClientListPager.sortIndex === 'ReferralName'" data-ng-class="{reverse:NoteClientListPager.reverse}"></span>
                                    </th>
                                    <th>
                                        <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteClientListPager.sortColumn('AHCCCSID')">@Resource.AHCCCSID</a>
                                        <span class="sortorder" data-ng-show="NoteClientListPager.sortIndex === 'AHCCCSID'" data-ng-class="{reverse:NoteClientListPager.reverse}"></span>
                                    </th>
                                    <th>
                                        <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteClientListPager.sortColumn('CISNumber')">@Resource.CISLabel</a>
                                        <span class="sortorder" data-ng-show="NoteClientListPager.sortIndex === 'CISNumber'" data-ng-class="{reverse:NoteClientListPager.reverse}"></span>
                                    </th>
                                    <th>
                                        <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteClientListPager.sortColumn('Payor')">@Resource.Payor</a>
                                        <span class="sortorder" data-ng-show="NoteClientListPager.sortIndex === 'Payor'" data-ng-class="{reverse:NoteClientListPager.reverse}"></span>
                                    </th>
                                    <th>
                                        <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteClientListPager.sortColumn('Population')">@Resource.Population</a>
                                        <span class="sortorder" data-ng-show="NoteClientListPager.sortIndex === 'Population'" data-ng-class="{reverse:NoteClientListPager.reverse}"></span>
                                    </th>
                                    <th>
                                        <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteClientListPager.sortColumn('Title')">@Resource.Title</a>
                                        <span class="sortorder" data-ng-show="NoteClientListPager.sortIndex === 'Title'" data-ng-class="{reverse:NoteClientListPager.reverse}"></span>
                                    </th>
                                    @*<th>
                                        <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteClientListPager.sortColumn('FacilityName')">@Resource.Facility</a>
                                        <span class="sortorder" data-ng-show="NoteClientListPager.sortIndex === 'FacilityName'" data-ng-class="{reverse:NoteClientListPager.reverse}"></span>
                                    </th>*@
                                    <th>
                                        <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteClientListPager.sortColumn('StarDate')">@Resource.StarDate</a>
                                        <span class="sortorder" data-ng-show="NoteClientListPager.sortIndex === 'StarDate'" data-ng-class="{reverse:NoteClientListPager.reverse}"></span>
                                    </th>
                                    <th class="width-80px">
                                        <span>@Resource.Action</span>
                                    </th>
                                </tr>
                            </thead>


                            <tbody dir-paginate="item in NoteClientList | itemsPerPage: NoteClientListPager.pageSize" total-items="NoteClientListPager.totalRecords" current-page="NoteClientListPager.currentPage" pagination-id="NoteClientListPagerID">

                                <tr>

                                    <td>
                                        <span class="collapseSource curser-pointer font-size-14 font-grey-salsa fa fa-plus-circle" data-toggle="collapse"
                                            data-ng-attr-data-target="{{'#ReferralDetails-'+item.ReferralID }}"
                                            data-ng-click="GetClientNoteDetailList(item,true,true)" data-ng-attr-id="{{'MEDocDetails-'+item.ReferralID }}"></span>
                                    </td>

                                    <td>
                                        <a class="" data-ng-href="/referral/addreferral/{{item.EncryptedReferralID}}" target="_blank">{{item.ReferralName}}</a>
                                    </td>
                                    <td>{{item.AHCCCSID}}</td>
                                    <td>{{item.CISNumber}}</td>
                                    <td>{{item.Payor}}</td>
                                    <td>{{item.Population}}</td>
                                    <td>{{item.Title}}</td>
                                    @*<td>{{item.FacilityName}}</td>*@
                                    <td>{{item.StrStartDate}}</td>
                                    <td>

                                        <a href="#" class="btn btn-xs blue" data-ng-click="OpenAddNoteModel(item)">@Resource.AddNote </a>

                                        @*<a class="btn btn-xs green-dark margin-left-10"  target="_blank" href="javascript:void(0);"
                                            data-ng-href="/referral/addreferral/{{item.EncryptedReferralID}}#rm_ReferralsMonthlySummary">@Resource.ViewSnapShots</a>*@
                                    </td>
                                </tr>

                                <tr data-ng-attr-id="{{'ReferralDetails-'+item.ReferralID }}" class="collapse collapseDestination schRef">
                                    <!--#region Sub CLIENT NOTE Details-->
                                    <td></td>
                                    <td colspan="9">

                                        <div class="row">
                                            <div class="col-sm-12 ">
                                                <div class="text-align-center margin-top-15">
                                                    <div class="top pagination-sm" data-ng-show="item.GetNoteClientListAjaxCall">
                                                        <b>@Resource.Loading</b>
                                                        <img src="~/Assets/images/lazyload.gif" />
                                                    </div>
                                                </div>
                                                <div class="portlet-body noteCompact padding-15" style="display: block;" data-ng-hide="item.GetNoteClientListAjaxCall">
                                                    <div class="dataTables_wrapper" data-ng-if="item.ClientNoteDetailList.length > 0">
                                                        <table class="table table-striped table-hover table-bordered table-condensed tblSubtable">
                                                            <thead>
                                                                <tr>
