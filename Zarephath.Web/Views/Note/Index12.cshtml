@model SetNoteListModel
@{
    ViewBag.Title = "Index";

    Layout = Model.IsPartial ? null : "~/Views/Shared/_Layout.cshtml";
    if (!Model.IsPartial)
    {
        ViewBag.SelectedMenuItem = Resource.SMI_AddBillingNote;
    }
}


@Html.Hidden("hdnSetNoteListPage", Common.SerializeObject(Model))

<div data-ng-controller="NoteController">
    @if (!Model.IsPartial)
    {
        <div class="page-bar">
            <h3 class="page-title">@Resource.NoteList
            </h3>
        </div>
    }
    <div class="row">
        <div class="col-sm-12 table-responsive overflow-i">
            <div>
                <button type="button" class="btn collapsed grey border-grey-cascade btn-xs" data-toggle="collapse" data-target="#filter-panel">
                    <i class="fa fa-sliders" aria-hidden="true"></i>&nbsp; @Resource.AllFilters 
                </button>

                <div class="btn-group pull-right btn-group-xs btn-group-devided">
                    <a href="#" data-ng-click="OpenAddNoteModel()" class="btn blue  border-blue-sharp" data-toggle="modal">
                        <i class="fa fa-plus">&nbsp;&nbsp;</i>@Resource.AddBillableNote
                    </a>

                </div>


                <div id="filter-panel" class="filter-panel collapse">
                    <div class="panel panel-default">
                        <div class="panel-body">
                            @*<div class="col-md-2 col-sm-2">
                                <h6 class="sort-title">@Resource.Sortby</h6>
                                <hr>
                                <ul class="sort-list-info">
                                    <li>
                                        <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('IsBillable')">@Resource.NoteType</a>
                                        <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'IsBillable'" data-ng-class="{reverse:NoteListPager.reverse}"></span>
                                    </li>
                                    <li>
                                        <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('ServiceDate')">@Resource.DateOfService</a>
                                        <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'ServiceDate'" data-ng-class="{reverse:NoteListPager.reverse}"></span>
                                    </li>

                                    <li>
                                        <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('CreatedDate')">@Resource.CreatedDate</a>
                                        <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'CreatedDate'" data-ng-class="{reverse:NoteListPager.reverse}"></span>
                                    </li>

                                    <li>
                                        <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('ModifiedDate')">@Resource.ModifiedDate</a>
                                        <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'ModifiedDate'" data-ng-class="{reverse:NoteListPager.reverse}"></span>
                                    </li>

                                </ul>
                            </div>*@
                            <div class="col-md-12 col-sm-12 no-padding">
                                <form>

                                    @if (!Model.IsPartial)
                                    {
                                        <div class="form-group col-lg-2 col-md-4 col-sm-4 col-xs-12">
                                            <label class="control-label">@Resource.Client</label>
                                            <div class="">
                                                <input type="text"
                                       token-input
                                       ng-val="TempSearchNoteModel.ReferralID"
                                       textfield='Name'
                                       valuefield='ReferralID'
                                       searchurl="{{GetReferralsURL}}"
                                       placeholder="@Resource.SearchClientInformation"
                                       tokenlimit='1'
                                       onresultsformatter='ReferralDetailResultsFormatter'                                       
                                       ontokenformatter="ReferralTokenFormatter"                                       
                                       autoSelectFirstResult="true"
                                       minChars="0"
                                       tokeninputobj="ReferralTokenObj"
                                       id="ReferralIDTkn"
                                       customclass="form-control input-sm"
                                       name="TempSearchNoteModel.ReferralID" />
                                            </div>
                                        </div>
                                    }
                                    

                                    <div class="form-group col-lg-2 col-md-4 col-sm-4 col-xs-6">
                                        <label class="control-label">Service Code</label>
                                        <div>
                                            <input type="text" token-input-local
                                                   ng-val="TempSearchNoteModel.ServiceCodeID"
                                                   textfield='ServiceCode'
                                                   valuefield='ServiceCodeID'
                                                   local-data="NoteModel.ServiceCodes | unique:'ServiceCodeID'"
                                                   placeholder="@Resource.EnterServiceCode"
                                                   tokenlimit='1'                                                
                                                   onresultsformatter='ServiceCodeResultsFormatter'                                   
                                                   ontokenformatter="ServiceCodeTokenFormatter"
                                                   id="SearchServiceCodeToken"
                                                   customclass="form-control input-sm " />
                                        </div>

                                    </div>
                                    <div class="form-group col-lg-2 col-md-4 col-sm-4 col-xs-6">
                                        <label class="control-label">Note Type</label>
                                        @Html.DropDownListFor(p => p.SearchNoteListModel.IsBillable, Model.NoteTypes.ToSelectList(Model.SearchNoteListModel.IsBillable.ToString() ?? "", "Name", "Value"),
                                                              new
                                                              {
                                                                  convert_to_number = "",
                                                                  id = "IsBillable",
                                                                  data_ng_model = "TempSearchNoteModel.IsBillable",
                                                                  data_ng_init = "TempSearchNoteModel.IsBillable=TempSearchNoteModel.IsBillable==''?null:TempSearchNoteModel.IsBillable",
                                                                  @class = "form-control input-sm",
                                                                  data_val = false
                                                              })
                                    </div>
                                    <div class="form-group col-lg-2 col-md-4 col-sm-4 col-xs-6">
                                        <label class="control-label">Service Code Type</label>
                                        @Html.DropDownListFor(p => p.SearchNoteListModel.ServiceCodeTypeID, Model.ServiceCodeTypes.ToSelectList(Model.SearchNoteListModel.ServiceCodeTypeID.ToString() ?? "", "ServiceCodeTypeName", "ServiceCodeTypeID", "All", "-1"),
                                                              new
                                                              {
                                                                  convert_to_number = "",
                                                                  id = "ServiceCodeTypeID",
                                                                  data_ng_model = "TempSearchNoteModel.ServiceCodeTypeID",
                                                                  data_ng_init = "TempSearchNoteModel.ServiceCodeTypeID=TempSearchNoteModel.ServiceCodeTypeID==''?null:TempSearchNoteModel.ServiceCodeTypeID",
                                                                  @class = "form-control input-sm",
                                                                  data_val = false
                                                              })
                                    </div>

                                    <div class="form-group col-lg-2 col-md-4 col-sm-4 col-xs-6">
                                        <label class="control-label">Note Completed?</label>
                                        @Html.DropDownListFor(p => p.SearchNoteListModel.IsCompleted, Model.CompletedFilter.ToSelectList(Model.SearchNoteListModel.IsCompleted.ToString() ?? "", "Name", "Value"),
                                                              new
                                                              {
                                                                  convert_to_number = "",
                                                                  id = "IsCompleted",
                                                                  data_ng_model = "TempSearchNoteModel.IsCompleted",
                                                                  data_ng_init = "TempSearchNoteModel.IsCompleted=TempSearchNoteModel.IsCompleted==''?null:TempSearchNoteModel.IsCompleted",
                                                                  @class = "form-control input-sm",
                                                                  data_val = false
                                                              })
                                    </div>

                                    <div class="form-group col-lg-2 col-md-4 col-sm-4 col-xs-6">
                                        <label class="control-label">Note Kind</label>
                                        @Html.DropDownListFor(p => p.SearchNoteListModel.NoteKind, Model.NoteKinds.ToSelectList(Model.SearchNoteListModel.NoteKind ?? "", "Value", "Value", "All", ""),
                                                                  new
                                                                      {
                                                                          id = "NoteKind",
                                                                          data_ng_model = "TempSearchNoteModel.NoteKind",
                                                                          data_ng_init = "TempSearchNoteModel.NoteKind=TempSearchNoteModel.NoteKind==''?null:TempSearchNoteModel.NoteKind",
                                                                          @class = "form-control input-sm",
                                                                          data_val = false
                                                                      })
                                    </div>
                                    <div class="clearfix">
                                        <div class="form-group col-lg-2 col-md-3 col-sm-3">
                                            <label class="control-label">Service From Date</label>
                                            <div class="input-group date" datepicker="TempSearchNoteModel.ServiceDateStart" ng-dateval="TempSearchNoteModel.ServiceDateStart" data-ng-maxdate="TempSearchNoteModel.ServiceDateEnd">
                                                <input class="form-control input-sm dateInputMask"
                                                       id="ServiceDateStart" maxlength="10" name="TempSearchNoteModel.ServiceDateStart" placeholder="@Resource.ServiceFromDate"
                                                       type="text">
                                                <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                            </div>
                                        </div>
                                        <div class="form-group col-lg-2 col-md-3 col-sm-3">
                                            <label class="control-label">Service To Date</label>
                                            <div class="input-group date" datepicker="TempSearchNoteModel.ServiceDateEnd" ng-dateval="TempSearchNoteModel.ServiceDateEnd" data-ng-mindate="TempSearchNoteModel.ServiceDateStart">
                                                <input class="form-control input-sm dateInputMask"
                                                       id="ServiceDateEnd" maxlength="10" name="TempSearchNoteModel.ServiceDateEnd"
                                                       type="text" placeholder="@Resource.ServiceToDate">
                                                <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                                            </div>
                                        </div>
                                        <div class="form-group col-lg-2 col-md-4 col-sm-4 col-xs-6 pull-right">                                            
                                            <div class="pull-left col-md-push-1 col-md-11 no-padding">
                                                <div class="col-md-6 no-right-padding">
                                                    <button type="submit" class="btn btn-sm btn-primary form-control" data-ng-click="SearchNote()">
                                                        @Resource.Search
                                                    </button>
                                                </div>
                                                <div class="col-md-6 no-right-padding">
                                                    <button class="btn btn-sm grey border-grey-cascade form-control" data-ng-click="ResetSearchFilter()">
                                                        @Resource.Reset
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    @*<div class="form-group col-lg-3 col-md-4 col-sm-4 col-xs-6">
                                            <label class="control-label">General Search</label>
                                            @Html.TextBoxFor(m => m.SearchNoteListModel.SearchText, new { data_ng_model = "TempSearchNoteModel.SearchText", @class = "form-control input-sm", placeholder = Resource.GeneralSerach, maxlength = 50 })
                                        </div>*@


                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="space20"></div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            @if (Common.HasPermission(Constants.Permission_Referral_Details))
            {
                <div class="btn-group" data-ng-show="NoteList.length > 0">
                    <span class="btn blue btn-sm grey border-grey-cascade" style="padding: 5px;">
                        <input id="SelectAllNote" class="unicheckbox text-align-center" type="checkbox" data-ng-model="SelectAllCheckbox" data-ng-change="SelectAll()" val="SelectAllCheckbox">
                        <label for="SelectAllNote"></label>
                    </span>
                </div>            
            }
            <div class="btn-group">
                <button class="btn blue btn-sm grey border-grey-cascade" data-ng-click="Refresh()">
                    <i class="fa fa-refresh"></i>&nbsp;&nbsp;@Resource.Refresh
                </button>
            </div>
            <div class="btn-group">
                @Html.DropDownListFor(m => m.SearchNoteListModel.IsDeleted, Model.DeleteFilter.ToSelectList(Convert.ToString(Model.SearchNoteListModel.IsDeleted), "Name", "Value", "", ""),
                                      new
                                      {
                                          id = "IsDeleted",
                                          data_ng_model = "TempSearchNoteModel.IsDeleted",
                                          data_ng_init = "TempSearchNoteModel.IsDeleted = TempSearchNoteModel.IsDeleted==null?'-1':TempSearchNoteModel.IsDeleted.toString()",
                                          @class = "form-control input-sm btn blue btn-sm grey border-grey-cascade",
                                          data_ng_change = "SearchNote()"
                                      })
            </div>
            @if (Common.HasPermission(Constants.Permission_Referral_Details))
            {
                <div class="btn-group" data-ng-if="SelectedNoteIds.length > 0">
                    <button type="button" data-ng-click="DeleteNote()" class="btn blue btn-sm grey border-grey-cascade">
                        <i class="fa fa-random margin-right-10"></i>{{TempSearchNoteModel.IsDeleted==0?'@Constants.DisableSelected' :(TempSearchNoteModel.IsDeleted==1?'@Constants.EnableSelected':'@Constants.InverseSelected')}}</button>
                </div>
            }
            <div class="inline-block pull-right" data-ng-show="NoteList.length > 0">
                <div class="pull-right top pagination-sm">
                    <dir-pagination-controls boundary-links="true" max-size="NoteListPager.maxSize" on-page-change="NoteListPager.pageChanged(newPageNumber)" pagination-id="NoteListPagerID"></dir-pagination-controls>
                </div>
            </div>
        </div>
        <div class="space20"></div>
    </div>

    <div class="row">
        <div class="col-sm-12">
            <div class="portlet-body noteCompact" style="display: block;">
                <div class="dataTables_wrapper" data-ng-if="NoteList.length > 0">
                    <table class="table table-striped table-hover table-bordered table-condensed table-checkable">
                        <thead>
                            <tr>
                                <th></th>
                                <th></th>
                                <th>
                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('IsBillable')">@Resource.Type</a>
                                    <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'IsBillable'" data-ng-class="{reverse:NoteListPager.reverse}"></span>
                                </th>
                                <th>
                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('Batch')">@Resource.Batch</a>
                                    <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'Batch'" data-ng-class="{reverse:NoteListPager.reverse}"></span>
                                </th>
                                <th>
                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('StartDate')">@Resource.StartDate</a>
                                    <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'StartDate'" data-ng-class="{reverse:NoteListPager.reverse}"></span>
                                </th>
                                <th>
                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('EndDate')">@Resource.EndDate</a>
                                    <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'EndDate'" data-ng-class="{reverse:NoteListPager.reverse}"></span>
                                </th>
                                <th colspan="2">
                                    <span class="anchor_color">@Resource.Service</span>
                                    <div class="pull-right btn-group">
                                        <a class="btn-xs dropdown-toggle sort-by anchor_color" href="#" type="button" data-toggle="dropdown" aria-expanded="false">
                                            @Resource.Sortby
                                            <i class="fa fa-angle-down"></i>
                                        </a>
                                        <ul class="dropdown-menu pull-right sort-list">
                                            <li>
                                                <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('ServiceCode')">@Resource.ServiceCode
                                                    <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'ServiceCode'" data-ng-class="{reverse:NoteListPager.reverse}"></span></a>
                                            </li>
                                            <li>
                                                <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('ServiceType')">@Resource.ServiceType
                                                    <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'ServiceType'" data-ng-class="{reverse:NoteListPager.reverse}"></span></a>
                                            </li>
                                            <li>
                                                <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('BillingProvider')">@Resource.BillingProvider
                                                    <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'BillingProvider'" data-ng-class="{reverse:NoteListPager.reverse}"></span></a>
                                            </li>

                                            <li>
                                                <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('RenderingProvider')">@Resource.RenderingProvider
                                                    <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'RenderingProvider'" data-ng-class="{reverse:NoteListPager.reverse}"></span></a>
                                            </li>
                                            <li>
                                                <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('Payor')">@Resource.Payor
                                                    <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'Payor'" data-ng-class="{reverse:NoteListPager.reverse}"></span></a>
                                            </li>
                                        </ul>

                                    </div>
                                </th>
                                <th>
                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('Unit')">@Resource.Units</a>
                                    <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'Unit'" data-ng-class="{reverse:NoteListPager.reverse}"></span>
                                </th>
                                <th>
                                    <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('Amount')">@Resource.Amount</a>
                                    <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'Amount'" data-ng-class="{reverse:NoteListPager.reverse}"></span>
                                </th>
                                <th class="col-sm-3" colspan="2">
                                    @Resource.NoteStatusSummary
                                    <div class="pull-right btn-group">
                                        <a class="btn-xs dropdown-toggle sort-by anchor_color" href="#" type="button" data-toggle="dropdown" aria-expanded="false">
                                            @Resource.Sortby
                                            <i class="fa fa-angle-down"></i>
                                        </a>
                                        <ul class="dropdown-menu pull-right sort-list">
                                            <li>
                                                <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('CreatedDate')">@Resource.CreatedDate
                                                    <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'CreatedDate'" data-ng-class="{reverse:NoteListPager.reverse}"></span></a>
                                            </li>
                                            <li>
                                                <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('CreatedBy')">@Resource.CreatedBy
                                                    <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'CreatedBy'" data-ng-class="{reverse:NoteListPager.reverse}"></span></a>
                                            </li>
                                            <li>
                                                <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('ModifiedDate')">@Resource.UpdatedDate
                                                    <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'ModifiedDate'" data-ng-class="{reverse:NoteListPager.reverse}"></span></a>
                                            </li>

                                            <li>
                                                <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('UpdatedBy')">@Resource.UpdatedBy
                                                    <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'UpdatedBy'" data-ng-class="{reverse:NoteListPager.reverse}"></span></a>
                                            </li>
                                            <li>
                                                <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('SignatureDate')">@Resource.SignatureDate
                                                    <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'SignatureDate'" data-ng-class="{reverse:NoteListPager.reverse}"></span></a>
                                            </li>
                                            <li>
                                                <a class="anchor_color" href="javascript:void(0);" data-ng-click="NoteListPager.sortColumn('SignatureBy')">@Resource.SignatureBy
                                                    <span class="sortorder" data-ng-show="NoteListPager.sortIndex === 'SignatureBy'" data-ng-class="{reverse:NoteListPager.reverse}"></span></a>
                                            </li>
                                        </ul>

                                    </div>
                                </th>

                                <th><span class="anchor_color">@Resource.Action</span></th>
                            </tr>
                        </thead>
                        <tbody dir-paginate="note in NoteList | itemsPerPage: NoteListPager.pageSize" total-items="NoteListPager.totalRecords" current-page="NoteListPager.currentPage" pagination-id="NoteListPagerID">
                            <tr data-ng-class="{'deleted':note.IsDeleted}">
                                <td>
                                    <span class="collapseSource curser-pointer font-size-18 font-blue fa fa-plus-circle" data-ng-click="NoteDetailsClick(note,'#NoteDetail-'+note.NoteID)" data-ng-attr-id="{{'NoteDetail-'+note.NoteID }}"
                                        data-toggle="collapse" data-ng-attr-data-target="{{'#NoteDetailID-'+note.NoteID }}"></span>
                                </td>
                                <td>
                                    @if (Common.HasPermission(Constants.Permission_CaseManager_AddUpdate))
                                    {
                                        <input type="checkbox" class="unicheckbox text-align-center" data-ng-model="note.IsChecked" data-ng-change="SelectNote(note)" val="note.IsChecked" />
                                    }
                                </td>
                                <td><span class="badge purple-medium" data-ng-bind="note.IsBillable?'B':'NB'" title="{{note.IsBillable?'@Resource.BillableNote':'@Resource.NonBillableNote'}}"></span></td>
                                <td>
                                    <span>{{note.BatchID?note.BatchID:'@Resource.NA'}}</span>
                                    <br />
                                    <span class="badge badge-danger" data-ng-if="note.IsSent">Sent</span>
                                </td>
                                <td data-ng-bind="note.StrStartDate"></td>
                                <td data-ng-bind="note.StrEndDate"></td>
                                <td data-ng-bind="note.ServiceCodeID>0?note.ServiceCode:'Other'"></td>
                                <td>
                                    <span>{{note.ServiceCodeID>0?note.ServiceName:note.Source?note.Source:note.OtherNoteType?note.OtherNoteType:'@Resource.NA'}}</span>
                                    <br />

                                    <div data-ng-if="note.ServiceCodeID>0">
                                        <div class="NoteSummaryDiv">
                                            <div class="td-note-summary">@Resource.ServiceType:</div>
                                            <div class="NoteServiceDesc">{{note.ServiceCodeTypeName}}</div>
                                        </div>
                                        <div class="clear-both">
                                            <div class="td-note-summary">@Resource.BillingProvider:</div>
                                            <div class="NoteServiceDesc">{{note.BillingProviderName}}</div>
                                        </div>
                                        <div class="clear-both">
                                            <div class="td-note-summary">@Resource.RenderingProvider:</div>
                                            <div class="NoteServiceDesc">{{note.RenderingProviderName}}</div>
                                        </div>
                                        <div class="clear-both">
                                            <div class="td-note-summary">@Resource.Payor:</div>
                                            <div class="NoteServiceDesc">{{note.PayorShortName?note.PayorShortName:'@Resource.NA'}}</div>
                                        </div>
                                    </div>


                                </td>
                                <td data-ng-bind="note.CalculatedUnit"></td>
                                <td data-ng-bind="note.CalculatedAmount | currency:'$'"></td>
                                <td>
                                    <div>
                                        <div class="NoteSummaryDiv">
                                            <div class="td-note-summary">@Resource.CreatedBy:</div>
                                            <div class="NoteServiceDesc">{{note.CreatedByUserName ? note.CreatedByUserName : @Resource.System}}</div>
                                        </div>
                                        <div>
                                            <div class="td-note-summary">@Resource.Date:</div>
                                            <div class="NoteServiceDesc">{{note.CreatedDate | datetimeformat}}</div>
                                        </div>
                                    </div>
                                    <hr />
                                    <div>
                                        <div class="NoteSummaryDiv">
                                            <div class="td-note-summary">@Resource.UpdatedBy:</div>
                                            <div class="NoteServiceDesc">{{note.UpdatedByUserName ? note.UpdatedByUserName : @Resource.System}}</div>
                                        </div>
                                        <div>
                                            <div class="td-note-summary">@Resource.Date:</div>
                                            <div class="NoteServiceDesc">{{note.UpdatedDate | datetimeformat}}</div>
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <div>
                                        <div class="NoteSummaryDiv">
                                            <div class="td-note-summary">@Resource.SignatureBy:</div>
                                            <div class="NoteServiceDesc">{{note.SignatureBy ? note.SignatureBy : '@Resource.NA'}}</div>
                                        </div>
                                        <div>
                                            <div class="td-note-summary">@Resource.Date:</div>
                                            <div class="NoteServiceDesc" data-ng-show="note.SignatureDate">{{note.SignatureDate | datetimeformat}}</div>
                                            <div class="NoteServiceDesc" data-ng-show="!note.SignatureDate">@Resource.NA</div>
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    @if (Common.HasPermission(Constants.Permission_CaseManager_AddUpdate))
                                    {
                                        <a href="#" class="btn btn-xs blue" data-ng-click="OpenAddNoteModel(note)">@Resource.Edit </a>
                                        <a href="javascript:void(0);" data-ng-show="note.IsDeleted" data-ng-click="DeleteNote(note,'Enable')" class="btn btn-xs green">@Resource.Enable</a>
                                        <a href="javascript:void(0);" data-ng-show="!note.IsDeleted" data-ng-click="DeleteNote(note,'Disable')" class="btn btn-xs red">@Resource.Disable</a>
                                    }
                                </td>
                            </tr>
                            <tr data-ng-attr-id="{{'NoteDetailID-'+note.NoteID }}" class="collapse border-grey-gallery collapseDestination">
                                <td></td>
                                <td colspan="11" class="">

                                    <div class="col-sm-10 margin-top-10">
                                        <div class="col-sm-12" data-ng-if="note.ServiceCodeType==OtherServiceCode">
                                            <span><b>Spoke To: </b>{{note.SpokeTo?note.SpokeTo:'@Resource.NA'}} </span>
                                            <span><b>Relationship: </b>{{note.Relation?note.Relation:'@Resource.NA'}} </span>
                                            <span><b>Type: </b>{{note.OtherNoteType?note.OtherNoteType:'@Resource.NA'}}</span>
                                        </div>
                                        <div class="table-responsive col-sm-12">
                                            <table class="table table-bordered table-condensed bg-gray-color">
                                                @*<thead>
                                                <tr>
                                                    <th colspan="2" class="text-align-left"><strong>@Resource.NoteDetails</strong></th>
                                                </tr>
                                            </thead>*@
                                                <tbody>
                                                    <tr>
                                                        <td class="col-sm-1 text-align-left">@Resource.Note</td>
                                                        <td data-ng-bind-html="note.NoteDetails"></td>
                                                        @*<td data-ng-bind="note.NoteDetails | trustAsHtml"></td>*@
                                                    </tr>
                                                    <tr>
                                                        <td class="text-align-left">@Resource.Assessment</td>
                                                        <td data-ng-bind-html="note.Assessment?note.Assessment:'@Resource.NA'"></td>
                                                    </tr>
                                                    <tr>
                                                        <td class="text-align-left">@Resource.ActionPlan</td>
                                                        <td data-ng-bind-html="note.ActionPlan?note.ActionPlan:'@Resource.NA'"></td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>

                                </td>

                            </tr>
                        </tbody>
                    </table>
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="col-md-4 no-padding pull-left">
                                @Resource.Showing <span data-ng-bind="(NoteListPager.pageSize * NoteListPager.currentPage) - NoteListPager.pageSize  + 1"></span>
                                &nbsp;@Resource.To <span data-ng-bind="(NoteListPager.pageSize == NoteListPager.currentPageSize) ? (NoteListPager.pageSize * NoteListPager.currentPage) :(NoteListPager.pageSize * NoteListPager.currentPage) - NoteListPager.pageSize  + NoteListPager.currentPageSize"></span>&nbsp;@Resource.Of &nbsp;<span data-ng-bind="NoteListPager.totalRecords"></span>&nbsp;@Resource.Records

                            </div>
                            <div class="inline-block pull-right" data-ng-show="NoteList.length > 0">
                                <div class="pull-right top pagination-sm">
                                    <dir-pagination-controls boundary-links="true" max-size="NoteListPager.maxSize" on-page-change="NoteListPager.pageChanged(newPageNumber)" pagination-id="NoteListPagerID"></dir-pagination-controls>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-sm-12">
            <div data-ng-if="NoteList.length == 0">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="alert alert-danger">
                            @Html.Raw(Resource.NoRecordsFound)
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>




