<div id="print-content-sparform" data-ng-show="PrintContent">
@*<div>*@
    <div class="row">
        <div class="col-xs-12">

            <div class="col-xs-6  pull-left">
                <img src="/Assets/images/logo-zrpath.png" alt="Logo" />
            </div>

            <div class="col-xs-6 pull-right">
                <label class="font-size-25 pull-right margin-top-15">@Resource.NewReferralSparForm</label>
            </div>

        </div>

        <form class="col-xs-12 form-inline print-font-custom no-padding">

            <div class="form-group col-xs-12 form-md-line-input">
                <div class="col-xs-2">
                    <label class="control-label pull-right">@Resource.ClientName</label>
                </div>
                <div class="col-xs-10 checklist-print-value">
                    <label class="control-label">{{ReferralDetail.ReferralName}}</label>
                </div>
            </div>

            <div class="form-group col-xs-12 form-md-line-input">
                <div class="col-xs-2">
                    <label class="control-label pull-right">@Resource.ClientDob</label>
                </div>
                <div class="col-xs-10 checklist-print-value">
                    <label class="control-label">{{ReferralDetail.DateOfBirth | dateformat}}</label>
                </div>
            </div>

            <div class="form-group col-xs-12 form-md-line-input">
                <div class="col-xs-2">
                    <label class="control-label pull-right">@Resource.ProviderAgency</label>
                </div>
                <div class="col-xs-10 checklist-print-value">
                    <label class="control-label">{{ReferralDetail.ReferringAgency ? ReferralDetail.ReferringAgency:'@Resource.NA'}}</label>
                </div>
            </div>

            <div class="form-group col-xs-12 form-md-line-input">
                <div class="col-xs-2">
                    <label class="control-label pull-right">@Resource.DateOfReview</label>
                </div>
                <div class="col-xs-10 checklist-print-value">
                    <label class="control-label" style="border-bottom: 1px dashed #000 !important;">{{ReferralSparForm.ReviewDate ? (ReferralSparForm.ReviewDate | dateformat) : '@Resource.NA'}}</label>
                </div>
            </div>

            <div class="form-group col-xs-12 form-md-line-input">
                <div class="col-xs-2">
                    <label class="control-label pull-right">@Resource.AdmissionDate</label>
                </div>
                <div class="col-xs-10 checklist-print-value">
                    <label class="control-label" style="border-bottom: 1px dashed #000 !important;">{{ReferralSparForm.AdmissionDate ? (ReferralSparForm.AdmissionDate | dateformat): '@Resource.NA'}}</label>
                </div>
            </div>
            <div class="space20"></div>
            <div class="col-xs-12">
                <strong class="sparform-lable">@Resource.AssessmentChecklist</strong>
            </div>
            <div class="col-xs-12 margin-left-10">
                <div class="col-xs-12 form-md-line-input">
                    <label>@Resource.DateOfAssessmentReviewing</label>
                    <span class="checklist-print-value inline margin-left-10" style="border-bottom: 1px dashed #000 !important; float: right; display: inline-block; width: 85%;">{{ReferralSparForm.AssessmentDate ? ReferralSparForm.AssessmentDate : '@Resource.NA'}}
                    </span>
                </div>
                <div class="col-xs-12 form-md-line-input">
                    <label>@Resource.SubmittedAssessmentCompletedText</label>
                    <div class="inline margin-left-10 chk pull-right">
                        <label class="checkbox-inline">
                            <input data-ng-checked="ReferralSparForm.AssessmentCompletedAndSignedByBHP == true" type="checkbox">
                            @Resource.Yes
                        </label>
                        <label class="checkbox-inline">
                            <input data-ng-checked="ReferralSparForm.AssessmentCompletedAndSignedByBHP == false || ReferralSparForm.AssessmentCompletedAndSignedByBHP == null" type="checkbox">
                            @Resource.No
                        </label>
                    </div>
                </div>
                <div class="col-xs-12 form-md-line-input"  >
                    <label>@Resource.AssessmentIdentifiesDtoDtsText</label>
                    <div class="inline margin-left-10 chk pull-right">
                        <label class="checkbox-inline">
                            <input data-ng-checked="ReferralSparForm.IdentifyDTSDTOBehavior == true" data-ng-true-value="true" data-ng-false-value="false" type="checkbox">
                            @Resource.Yes
                        </label>
                        <label class="checkbox-inline">
                            <input data-ng-checked="ReferralSparForm.IdentifyDTSDTOBehavior == false || ReferralSparForm.IdentifyDTSDTOBehavior == null" data-ng-true-value="false" data-ng-false-value="true" type="checkbox">
                            @Resource.No
                        </label>
                    </div>
                </div>
