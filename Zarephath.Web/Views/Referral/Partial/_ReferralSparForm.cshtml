@model SetReferralSparFormModel
@{
    // For: on reset fields, will set current date
    if (Model.ReferralSparForm.DemographicDate == DateTime.MinValue)
    {
        Model.ReferralSparForm.DemographicDate = DateTime.Now;
    }
    if (Model.ReferralSparForm.Date == DateTime.MinValue)
    {
        Model.ReferralSparForm.Date = DateTime.Now;
    }
}
@Html.Hidden("hdnSetReferralSparFormModel", Common.SerializeObject(Model))

<form id="frmreferralsparform">

    <div class="row">

        <div class="col-md-6 col-sm-6 padding-left-25">
            <span class="badge  badge-success" data-ng-if="ReferralSparForm.IsSparFormOffline">
                <strong>@Resource.FormIsOffline</strong>
            </span>
        </div>

        <div class="col-md-6 col-sm-6 pull-right no-print text-align-right">
            <a class="btn btn-sm btn-primary margin-right-10 yellow" data-ng-click="PrintDiv('print-content-sparform')" title="Print"><i class="fa fa-print"></i>&nbsp;@Resource.Print</a>
            @if (Common.HasPermission(Constants.Permission_ReferralSparForm_AddUpdate))
            {
                <button data-ng-click="ResetReferralSparForm()" class="btn btn-sm red">
                    <i class="fa fa-remove">&nbsp;&nbsp;</i>@Resource.Reset
                </button>
                <button data-ng-click="SaveReferralSparForm()" class="btn btn-sm green-jungle">
                    <i class="fa fa-save">&nbsp;&nbsp;</i>@Resource.Save
                </button>
            }

        </div>

        <div class="col-md-12 col-sm-12 content-sparform">
            <div>

                <div class="form-group col-md-12 col-sm-12 form-md-line-input">
                    <label class="col-md-2 col-sm-3 control-label">@Resource.ClientName</label>
                    <div class="col-md-8 col-sm-8">
                        @Html.TextBoxFor(m => m.ReferralForReferralSparFormModel.ReferralName, new
                   {
                       data_ng_model = "ReferralDetail.ReferralName",
                       @class = "form-control input-sm prevent-enable",
                       disabled = "disabled"
                   })
                        @*<input type="text" class="form-control input-sm" id="Text65" name="inputMiddleName" disabled="disabled">*@
                    </div>
                </div>

                <div class="form-group col-md-12 col-sm-12 form-md-line-input">
                    <label class="col-md-2 col-sm-3 control-label">@Resource.ClientDob</label>
                    <div class="col-md-8 col-sm-8" datepicker="ReferralDetail.DateOfBirth" ng-dateval="ReferralDetail.DateOfBirth">
                        @Html.TextBoxFor(m => m.ReferralForReferralSparFormModel.DateOfBirth, new
                   {
                       @class = "form-control input-sm datepicker prevent-enable",
                       disabled = "disabled",
                       date_mask = ""
                   })
                    </div>
                </div>

                <div class="form-group col-md-12 col-sm-12 form-md-line-input">
                    <label class="col-md-2 col-sm-3 control-label">@Resource.ProviderAgency</label>
                    <div class="col-md-8 col-sm-8">
                        @Html.TextBoxFor(m => m.ReferralForReferralSparFormModel.ReferringAgency, new
                   {
                       data_ng_model = "ReferralDetail.ReferringAgency",
                       @class = "form-control input-sm prevent-enable",
                       disabled = "disabled"
                   })
                    </div>
                </div>

                <div class="form-group col-md-12 col-sm-12 form-md-line-input">
                    <label class="col-md-2 col-sm-3 control-label">@Resource.DateOfReview</label>
                    <div class="col-md-8 col-sm-8" datepicker="ReferralSparForm.ReviewDate" ng-dateval="ReferralSparForm.ReviewDate">
                        @Html.TextBoxFor(m => m.ReferralSparForm.ReviewDate, new { @class = "form-control input-sm dateInputMask", maxlength = "10", date_mask = "" })
                    </div>
                </div>

                <div class="form-group col-md-12 col-sm-12 form-md-line-input">
                    <label class="col-md-2 col-sm-3 control-label">@Resource.AdmissionDate</label>
                    <div class="col-md-8 col-sm-8" datepicker="ReferralSparForm.AdmissionDate" ng-dateval="ReferralSparForm.AdmissionDate">
                        @Html.TextBoxFor(m => m.ReferralSparForm.AdmissionDate, new { @class = "form-control input-sm prevent-enable", maxlength = "10", date_mask = "", disabled = "disabled" })
                    </div>
                </div>

                <div class="space20"></div>

                <div class="form-group col-md-12 col-sm-12 form-md-line-input">
                    <strong class="sparform-lable">@Resource.AssessmentChecklist</strong>
                </div>

                <div class="form-group col-md-12 col-sm-12 form-md-line-input">
