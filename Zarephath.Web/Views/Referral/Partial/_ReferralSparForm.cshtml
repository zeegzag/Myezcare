                    <div class="col-md-5 col-md-5">
                        <div class="mt-checkbox-inline">
                            <!-- <label class="mt-checkbox mt-checkbox-outline">
                                     <input data-ng-model="ReferralSparForm.IdentifyDTSDTOBehavior" ng-true-value="true" ng-false-value="false" name="AssessmentIdentifiesDtoDtsText" type="checkbox">@*class="unicheckbox" val="ReferralSparForm.IdentifyDTSDTOBehavior"*@
                                     @Resource.Yes
                                     <span></span>
                                 </label>
                                <label class="mt-checkbox mt-checkbox-outline">
                                    <input data-ng-model="ReferralSparForm.IdentifyDTSDTOBehavior" ng-true-value="false" ng-false-value="true" name="AssessmentIdentifiesDtoDtsText" type="checkbox">@*class="unicheckbox" val="ReferralSparForm.IdentifyDTSDTOBehavior"*@
                                    @Resource.No
