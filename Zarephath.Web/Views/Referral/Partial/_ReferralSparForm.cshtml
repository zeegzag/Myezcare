                            <label class="mt-checkbox mt-checkbox-outline">
                                <input data-ng-model="ReferralSparForm.IdentifyDTSDTOBehavior"
                                    class="uniformControls" data-uniform-value="!ReferralSparForm.IdentifyDTSDTOBehavior"
                                    data-ng-true-value="false" data-ng-false-value="true"
                                    type="checkbox">@Resource.No<span></span>
                            </label>

                        </div>
                    </div>
                </div>

                <div class="form-group col-md-12 col-sm-12 form-md-line-input padding-top-20">
                    @*<label class="col-md-2 control-label">@Resource.DemographicDate:</label>*@

                    @*<div class="col-md-3">
                        <label class="control-label bold">@Resource.DemographicDate</label>
                        <div datepicker="ReferralSparForm.DemographicDate" ng-dateval="ReferralSparForm.DemographicDate">
                            @Html.TextBoxFor(m => m.ReferralSparForm.DemographicDate, new { @class = "form-control input-sm dateInputMask" })
                        </div>
                    </div>*@

                    <label class="col-md-2 control-label bold">@Resource.CASIIScore</label>
                    <div class="col-md-5">
                        @Html.TextBoxFor(m => m.ReferralSparForm.CASIIScore, new
                   {
                       @class = "form-control input-sm",
                       dir_valid_number = "",
                       data_ng_model = "ReferralSparForm.CASIIScore",
                       @placeholder = Resource.CASIIScore,
                       maxlength = "10"
                   })
                    </div>



                    <div class="col-md-5 col-md-5 ">
                        <div class="mt-checkbox-inline">
                            @* <label class="mt-checkbox mt-checkbox-outline">
                                <input type="checkbox" data-ng-model="ReferralSparForm.IsROI" class="unicheckbox" val="ReferralSparForm.IsROI">@Resource.ROI
                                <span></span>
                            </label>
                            <label class="mt-checkbox mt-checkbox-outline">
                                <input type="checkbox" data-ng-model="ReferralSparForm.IsSNCD" class="unicheckbox" val="ReferralSparForm.IsSNCD">
                                @Resource.SNCD
                                <span></span>
                            </label>*@


                            <label class="mt-checkbox mt-checkbox-outline display-none">
                                <input data-ng-model="ReferralSparForm.IsROI"
                                    class="uniformControls " data-uniform-value="ReferralSparForm.IsROI"
                                    data-ng-true-value="true" data-ng-false-value="false"
                                    type="checkbox">@Resource.ROI<span></span>
                            </label>
                            <label class="mt-checkbox mt-checkbox-outline">
                                <input data-ng-model="ReferralSparForm.IsSNCD"
                                    class="uniformControls" data-uniform-value="ReferralSparForm.IsSNCD"
                                    type="checkbox" />
                                @Resource.SNCD
                            </label>
                        </div>
                    </div>
                </div>

                <div class="span12"></div>

                <div class="form-group col-md-12 col-sm-12 form-md-line-input">
                    <label class="bold col-md-2 control-label">@Resource.UpdatesIfApplicable</label>
                    <div class="col-md-12">
                        @Html.TextAreaFor(m => m.ReferralSparForm.DTSDTOUpdateText, new { data_ng_model = "ReferralSparForm.DTSDTOUpdateText", @class = "form-control input-sm", rows = "3", maxlength = "500" })
                    </div>
                </div>

                <div class="form-group col-md-12 col-sm-12 form-md-line-input">
                    <label class="bold col-md-3 control-label">@Resource.AdditionalInformation</label>
                    <div class="col-md-12">
                        @Html.TextAreaFor(m => m.ReferralSparForm.AdditionInformation, new { data_ng_model = "ReferralSparForm.AdditionInformation", @class = "form-control input-sm", rows = "3", maxlength = "500" })
                    </div>
                </div>

                <div class="space20"></div>

                <div class="form-group col-md-12 col-sm-12 form-md-line-input">
                    <strong class="sparform-lable">@Resource.ServicePlanChecklist</strong>
                </div>

                <div class="form-group col-md-12 col-sm-12 form-md-line-input">
                    <label class="col-md-7 col-sm-7 control-label">@Resource.ServicePlanSignedByGuardian</label>
                    <div class="col-md-5 col-sm-5">
                        <div class="mt-checkbox-inline">
                            <label class="mt-checkbox mt-checkbox-outline">
                                <input data-ng-model="ReferralSparForm.ServicePlanCompleted"
                                    class="uniformControls" data-uniform-value="ReferralSparForm.ServicePlanCompleted"
                                    data-ng-true-value="true" data-ng-false-value="false"
                                    type="checkbox">@Resource.Yes<span></span>
                            </label>
                            <label class="mt-checkbox mt-checkbox-outline">
                                <input data-ng-model="ReferralSparForm.ServicePlanCompleted"
                                    class="uniformControls" data-uniform-value="!ReferralSparForm.ServicePlanCompleted"
                                    data-ng-true-value="false" data-ng-false-value="true"
                                    type="checkbox">@Resource.No<span></span>
                            </label>
                        </div>
                    </div>
                </div>

                <div class="form-group col-md-12 col-sm-12 form-md-line-input">
                    <label class="col-md-7 col-sm-7 control-label">@Resource.ServicePlanSignedAndDatedByBHP</label>
                    <div class="col-md-5 col-sm-5">
                        <div class="mt-checkbox-inline">
                            <label class="mt-checkbox mt-checkbox-outline">
                                <input data-ng-model="ReferralSparForm.ServicePlanSignedDatedByBHP"
                                    class="uniformControls" data-uniform-value="ReferralSparForm.ServicePlanSignedDatedByBHP"
                                    data-ng-true-value="true" data-ng-false-value="false"
                                    type="checkbox">@Resource.Yes<span></span>
                            </label>
                            <label class="mt-checkbox mt-checkbox-outline">
                                <input data-ng-model="ReferralSparForm.ServicePlanSignedDatedByBHP"
                                    class="uniformControls" data-uniform-value="!ReferralSparForm.ServicePlanSignedDatedByBHP"
                                    data-ng-true-value="false" data-ng-false-value="true"
                                    type="checkbox">@Resource.No<span></span>
                            </label>
                        </div>
                    </div>
                </div>

                <div class="form-group col-md-12 col-sm-12 form-md-line-input">
                    <label class="col-md-7 col-sm-7 control-label">@Resource.ServicePlanHasRespiteCounselingLifeSkills</label>
                    <div class="col-md-5 col-sm-5">
                        <div class="mt-checkbox-inline">
                            <label class="mt-checkbox mt-checkbox-outline">
                                <input data-ng-model="ReferralSparForm.ServicePlanIdentified"
                                    class="uniformControls" data-uniform-value="ReferralSparForm.ServicePlanIdentified"
                                    data-ng-true-value="true" data-ng-false-value="false"
                                    type="checkbox">@Resource.Yes<span></span>
                            </label>
                            <label class="mt-checkbox mt-checkbox-outline">
                                <input data-ng-model="ReferralSparForm.ServicePlanIdentified"
                                    class="uniformControls" data-uniform-value="!ReferralSparForm.ServicePlanIdentified"
                                    data-ng-true-value="false" data-ng-false-value="true"
                                    type="checkbox">@Resource.No<span></span>
                            </label>

                        </div>

                    </div>
                </div>

                <div class="form-group col-md-12 col-sm-12 form-md-line-input">
                    <label class="col-md-7 col-sm-7 control-label">@Resource.ServicePlanHasFrequencyOfTheIdentified</label>
                    <div class="col-md-5 col-sm-5">

                        <div class="mt-checkbox-inline">
                            <label class="mt-checkbox mt-checkbox-outline">
                                <input data-ng-model="ReferralSparForm.ServicePlanHasFrequency"
                                    class="uniformControls" data-uniform-value="ReferralSparForm.ServicePlanHasFrequency"
                                    data-ng-true-value="true" data-ng-false-value="false"
                                    type="checkbox">@Resource.Yes<span></span>
                            </label>
                            <label class="mt-checkbox mt-checkbox-outline">
                                <input data-ng-model="ReferralSparForm.ServicePlanHasFrequency"
                                    class="uniformControls" data-uniform-value="!ReferralSparForm.ServicePlanHasFrequency"
                                    data-ng-true-value="false" data-ng-false-value="true"
                                    type="checkbox">@Resource.No<span></span>
                            </label>

                        </div>

                    </div>
                </div>

                <div class="form-group col-md-12 col-sm-12 form-md-line-input">
                    <label class="bold col-md-3 control-label">@Resource.AdditionalInformation</label>
                    <div class="col-md-12">
                        @Html.TextAreaFor(m => m.ReferralSparForm.ServicePlanAdditionalInfo, new { data_ng_model = "ReferralSparForm.ServicePlanAdditionalInfo", @class = "form-control input-sm", rows = "3", maxlength = "500" })
                    </div>
                </div>

                <div class="form-group col-md-12 col-sm-12 form-md-line-input display-none">
                    <div class="col-md-6 col-sm-6">
                        @Html.TextBoxFor(m => m.ReferralSparForm.BHPReviewSignature, new { data_ng_model = "ReferralSparForm.BHPReviewSignature", @class = "form-control input-sm" })
                    </div>
                    <div class="col-md-5" datepicker="ReferralSparForm.Date" ng-dateval="ReferralSparForm.Date">
                        @Html.TextBoxFor(m => m.ReferralSparForm.Date, new { @class = "form-control input-sm dateInputMask  pull-right", maxlength = "10" })
                    </div>
                </div>

                <div class="form-group col-md-12 col-sm-12 form-md-line-input display-none">
                    <label class="col-md-6 col-sm-3 control-label">@Resource.BHPReviewerSignatureCredentials</label>
                    <label class="col-md-6 col-sm-3 center-block control-label">@Resource.Date</label>
                </div>

            </div>
        </div>

    </div>

    <div class="col-md-6 col-sm-6">
        <div class="col-md-12 margin-bottom-5">
            <div class="col-sm-1 no-left-padding">
                <input type="checkbox" id="markoffline" data-ng-model="ReferralSparForm.IsSparFormOffline" class="unicheckbox" val="ReferralSparForm.IsSparFormOffline">
            </div>
            <div class="col-sm-5 no-left-padding">
                <label for="markoffline"><strong class="markcomplete">@Resource.MarkAsOffline</strong></label>
            </div>
        </div>
        <div class="col-md-12">
            <div class="col-sm-1 no-left-padding">
                <input type="checkbox" id="markcomplete" data-ng-model="ReferralSparForm.IsSparFormCompleted" class="unicheckbox" val="ReferralSparForm.IsSparFormCompleted">
            </div>
            <div class="col-sm-5 no-left-padding">
                <label for="markcomplete"><strong class="markcomplete">@Resource.MarkAsComplete</strong></label>
            </div>
        </div>

    </div>

    <div class="col-md-6 col-sm-6 text-right">
        <a class="btn btn-sm btn-primary margin-right-10 yellow" data-ng-click="PrintDiv('print-content-sparform')" title="Print"><i class="fa fa-print"></i>&nbsp;@Resource.Print</a>
        @if (Common.HasPermission(Constants.Permission_ReferralSparForm_AddUpdate))
        {
            <button data-ng-click="ResetReferralSparForm()" class="btn btn-sm red">
                <i class="fa fa-remove">&nbsp;&nbsp;</i>@Resource.Reset
            </button>
            <button data-ng-click="SaveReferralSparForm()" class="btn btn-sm green-jungle">
                <i class="fa fa-save">&nbsp;&nbsp;</i>@Resource.Save
            </button>
        }
    </div>

</form>
