    <DataSet Name="ReferralAllergyList">
      <Query>
        <DataSourceName>DS2</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ReferralID">
            <Value>=Parameters!ReferralID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT STRING_AGG ( ISNULL(Allergy,'N/A'), ',') AS Allergy, STRING_AGG ( ISNULL(Reaction,'N/A'), ',') AS Reaction
FROM Allergy A                     
WHERE A.Patient = @ReferralID
AND IsDeleted=0</CommandText>
      </Query>
      <Fields>
        <Field Name="Allergy">
          <DataField>Allergy</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Reaction">
          <DataField>Reaction</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CareType">
      <Query>
        <DataSourceName>DS2</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ReferralID">
            <Value>=Parameters!ReferralID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT  STRING_AGG ( ISNULL(DM.Title,'N/A'), ',') AS CareType
FROM Referrals R                    
INNER JOIN DDMaster DM ON DM.DDMasterID IN (select val from GetCSVTable(R.CareTypeIds))
WHERE R.IsDeleted=0 AND R.ReferralID = @ReferralID</CommandText>
      </Query>
      <Fields>
        <Field Name="CareType">
          <DataField>CareType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ReferralList">
      <Query>
        <DataSourceName>DS2</DataSourceName>
        <CommandText>WITH CTE AS (
	SELECT 0 AS ReferralID,'select' AS ReferralName
	UNION ALL
	SELECT DISTINCT ReferralID,LastName + ', ' + FirstName AS ReferralName 
	FROM dbo.Referrals e where IsDeleted=0)
	SELECT * FROM CTE
	WHERE ReferralName IS NOT NULL
	ORDER BY CASE WHEN ReferralName = 'All' THEN '0'
		ELSE ReferralName END ASC;</CommandText>
      </Query>
      <Fields>
        <Field Name="ReferralID">
          <DataField>ReferralID</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="ReferralName">
          <DataField>ReferralName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
